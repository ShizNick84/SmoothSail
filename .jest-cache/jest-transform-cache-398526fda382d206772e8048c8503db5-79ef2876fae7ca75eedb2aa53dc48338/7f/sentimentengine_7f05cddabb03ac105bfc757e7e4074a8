    (cov_1btwfziq8q().s[85]++, this.sentimentHistory.slice(-12)); // Last hour (12 * 5-minute intervals)
    /* istanbul ignore next */
    cov_1btwfziq8q().s[86]++;
    if (recentHistory.length < 3) {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[14][0]++;
      cov_1btwfziq8q().s[87]++;
      return trends;
    } else
    /* istanbul ignore next */
    {
      cov_1btwfziq8q().b[14][1]++;
    }
    // Calculate trend direction and strength
    const scores =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[88]++, recentHistory.map(h => {
      /* istanbul ignore next */
      cov_1btwfziq8q().f[8]++;
      cov_1btwfziq8q().s[89]++;
      return h.aggregatedScore;
    }));
    const timeSpan =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[90]++, recentHistory.length * 5); // minutes
    // Linear regression for trend direction
    const {
      slope,
      correlation
    } =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[91]++, this.calculateTrendSlope(scores));
    let direction =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[92]++, 'NEUTRAL');
    /* istanbul ignore next */
    cov_1btwfziq8q().s[93]++;
    if (
    /* istanbul ignore next */
    (cov_1btwfziq8q().b[16][0]++, slope > 1) &&
    /* istanbul ignore next */
    (cov_1btwfziq8q().b[16][1]++, correlation > 0.5)) {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[15][0]++;
      cov_1btwfziq8q().s[94]++;
      direction = 'BULLISH';
    } else {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[15][1]++;
      cov_1btwfziq8q().s[95]++;
      if (
      /* istanbul ignore next */
      (cov_1btwfziq8q().b[18][0]++, slope < -1) &&
      /* istanbul ignore next */
      (cov_1btwfziq8q().b[18][1]++, correlation > 0.5)) {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[17][0]++;
        cov_1btwfziq8q().s[96]++;
        direction = 'BEARISH';
      } else
      /* istanbul ignore next */
      {
        cov_1btwfziq8q().b[17][1]++;
      }
    }
    const strength =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[97]++, Math.min(100, Math.abs(slope) * Math.abs(correlation) * 10));
    const changeRate =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[98]++, slope * 12); // Change per hour
    // Detect significant events
    const significantEvents =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[99]++, this.detectSignificantEvents(recentHistory));
    /* istanbul ignore next */
    cov_1btwfziq8q().s[100]++;
    trends.push({
      direction,
      strength: Math.round(strength),
      duration: timeSpan,
      changeRate: Math.round(changeRate * 100) / 100,
      significantEvents
    });
    /* istanbul ignore next */
    cov_1btwfziq8q().s[101]++;
    return trends;
  }
  /**
   * Calculate trend slope using linear regression
   */
  calculateTrendSlope(scores) {
    /* istanbul ignore next */
    cov_1btwfziq8q().f[9]++;
    const n =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[102]++, scores.length);
    const x =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[103]++, Array.from({
      length: n
    }, (_, i) => {
      /* istanbul ignore next */
      cov_1btwfziq8q().f[10]++;
      cov_1btwfziq8q().s[104]++;
      return i;
    }));
    const sumX =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[105]++, x.reduce((a, b) => {
      /* istanbul ignore next */
      cov_1btwfziq8q().f[11]++;
      cov_1btwfziq8q().s[106]++;
      return a + b;
    }, 0));
    const sumY =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[107]++, scores.reduce((a, b) => {
      /* istanbul ignore next */
      cov_1btwfziq8q().f[12]++;
      cov_1btwfziq8q().s[108]++;
      return a + b;
    }, 0));
    const sumXY =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[109]++, x.reduce((sum, xi, i) => {
      /* istanbul ignore next */
      cov_1btwfziq8q().f[13]++;
      cov_1btwfziq8q().s[110]++;
      return sum + xi * scores[i];
    }, 0));
    const sumXX =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[111]++, x.reduce((sum, xi) => {
      /* istanbul ignore next */
      cov_1btwfziq8q().f[14]++;
      cov_1btwfziq8q().s[112]++;
      return sum + xi * xi;
    }, 0));
    const sumYY =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[113]++, scores.reduce((sum, yi) => {
      /* istanbul ignore next */
      cov_1btwfziq8q().f[15]++;
      cov_1btwfziq8q().s[114]++;
      return sum + yi * yi;
    }, 0));
    const slope =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[115]++, (n * sumXY - sumX * sumY) / (n * sumXX - sumX * sumX));
    // Calculate correlation coefficient
    const numerator =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[116]++, n * sumXY - sumX * sumY);
    const denominator =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[117]++, Math.sqrt((n * sumXX - sumX * sumX) * (n * sumYY - sumY * sumY)));
    const correlation =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[118]++, denominator !== 0 ?
    /* istanbul ignore next */
    (cov_1btwfziq8q().b[19][0]++, numerator / denominator) :
    /* istanbul ignore next */
    (cov_1btwfziq8q().b[19][1]++, 0));
    /* istanbul ignore next */
    cov_1btwfziq8q().s[119]++;
    return {
      slope:
      /* istanbul ignore next */
      (cov_1btwfziq8q().b[20][0]++, slope) ||
      /* istanbul ignore next */
      (cov_1btwfziq8q().b[20][1]++, 0),
      correlation:
      /* istanbul ignore next */
      (cov_1btwfziq8q().b[21][0]++, correlation) ||
      /* istanbul ignore next */
      (cov_1btwfziq8q().b[21][1]++, 0)
    };
  }
  /**
   * Detect significant sentiment events
   */
  detectSignificantEvents(history) {
    /* istanbul ignore next */
    cov_1btwfziq8q().f[16]++;
    const events =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[120]++, []);
    const thresholds =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[121]++, this.config.thresholds);
    /* istanbul ignore next */
    cov_1btwfziq8q().s[122]++;
    for (let i =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[123]++, 1); i < history.length; i++) {
      const current =
      /* istanbul ignore next */
      (cov_1btwfziq8q().s[124]++, history[i]);
      const previous =
      /* istanbul ignore next */
      (cov_1btwfziq8q().s[125]++, history[i - 1]);
      const change =
      /* istanbul ignore next */
      (cov_1btwfziq8q().s[126]++, current.aggregatedScore - previous.aggregatedScore);
      // Rapid change detection
      /* istanbul ignore next */
      cov_1btwfziq8q().s[127]++;
      if (Math.abs(change) > thresholds.rapidChange) {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[22][0]++;
        cov_1btwfziq8q().s[128]++;
        events.push(`Rapid sentiment ${change > 0 ?
        /* istanbul ignore next */
        (cov_1btwfziq8q().b[23][0]++, 'increase') :
        /* istanbul ignore next */
        (cov_1btwfziq8q().b[23][1]++, 'decrease')}: ${change.toFixed(1)} points`);
      } else
      /* istanbul ignore next */
      {
        cov_1btwfziq8q().b[22][1]++;
      }
      // Extreme sentiment detection
      cov_1btwfziq8q().s[129]++;
      if (current.aggregatedScore > thresholds.extremePositive) {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[24][0]++;
        cov_1btwfziq8q().s[130]++;
        events.push(`Extreme positive sentiment: ${current.aggregatedScore.toFixed(1)}`);
      } else {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[24][1]++;
        cov_1btwfziq8q().s[131]++;
        if (current.aggregatedScore < thresholds.extremeNegative) {
          /* istanbul ignore next */
          cov_1btwfziq8q().b[25][0]++;
          cov_1btwfziq8q().s[132]++;
          events.push(`Extreme negative sentiment: ${current.aggregatedScore.toFixed(1)}`);
        } else
        /* istanbul ignore next */
        {
          cov_1btwfziq8q().b[25][1]++;
        }
      }
      // Volume spike detection
      const totalVolume =
      /* istanbul ignore next */
      (cov_1btwfziq8q().s[133]++, current.sources.reduce((sum, source) => {
        /* istanbul ignore next */
        cov_1btwfziq8q().f[17]++;
        cov_1btwfziq8q().s[134]++;
        return sum + source.volume;
      }, 0));
      /* istanbul ignore next */
      cov_1btwfziq8q().s[135]++;
      if (totalVolume > thresholds.volumeSpike) {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[26][0]++;
        cov_1btwfziq8q().s[136]++;
        events.push(`High activity volume: ${totalVolume} total items`);
      } else
      /* istanbul ignore next */
      {
        cov_1btwfziq8q().b[26][1]++;
      }
    }
    /* istanbul ignore next */
    cov_1btwfziq8q().s[137]++;
    return events;
  }
  /**
