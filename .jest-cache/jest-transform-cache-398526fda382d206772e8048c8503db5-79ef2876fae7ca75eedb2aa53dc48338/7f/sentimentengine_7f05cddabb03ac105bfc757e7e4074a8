  }
  /**
   * Calculate position sizing adjustment based on sentiment
   */
  calculateSentimentPositionAdjustment(baseSizePercent) {
    /* istanbul ignore next */
    cov_1btwfziq8q().f[26]++;
    cov_1btwfziq8q().s[182]++;
    if (this.sentimentHistory.length === 0) {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[44][0]++;
      cov_1btwfziq8q().s[183]++;
      return baseSizePercent;
    } else
    /* istanbul ignore next */
    {
      cov_1btwfziq8q().b[44][1]++;
    }
    const currentSentiment =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[184]++, this.sentimentHistory[this.sentimentHistory.length - 1]);
    const score =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[185]++, currentSentiment.aggregatedScore);
    const confidence =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[186]++, currentSentiment.confidence);
    // Base adjustment on sentiment score and confidence
    let adjustment =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[187]++, 1.0);
    /* istanbul ignore next */
    cov_1btwfziq8q().s[188]++;
    if (confidence > 60) {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[45][0]++;
      cov_1btwfziq8q().s[189]++;
      if (score > 30) {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[46][0]++;
        cov_1btwfziq8q().s[190]++;
        // Positive sentiment - increase position size
        adjustment = 1 + score / 100 * 0.5; // Up to 50% increase
      } else {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[46][1]++;
        cov_1btwfziq8q().s[191]++;
        if (score < -30) {
          /* istanbul ignore next */
          cov_1btwfziq8q().b[47][0]++;
          cov_1btwfziq8q().s[192]++;
          // Negative sentiment - decrease position size
          adjustment = 1 + score / 100 * 0.3; // Up to 30% decrease
        } else
        /* istanbul ignore next */
        {
          cov_1btwfziq8q().b[47][1]++;
        }
      }
    } else
    /* istanbul ignore next */
    {
      cov_1btwfziq8q().b[45][1]++;
    }
    // Apply confidence scaling
    const confidenceScale =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[193]++, confidence / 100);
    /* istanbul ignore next */
    cov_1btwfziq8q().s[194]++;
    adjustment = 1 + (adjustment - 1) * confidenceScale;
    // Ensure reasonable bounds
    /* istanbul ignore next */
    cov_1btwfziq8q().s[195]++;
    adjustment = Math.max(0.5, Math.min(2.0, adjustment));
    /* istanbul ignore next */
    cov_1btwfziq8q().s[196]++;
    return baseSizePercent * adjustment;
  }
  /**
   * Create empty sentiment score for error cases
   */
  createEmptySentimentScore(source) {
    /* istanbul ignore next */
    cov_1btwfziq8q().f[27]++;
    cov_1btwfziq8q().s[197]++;
    return {
      source,
      score: 0,
      confidence: 0,
      volume: 0,
      timestamp: new Date(),
      keyTopics: []
    };
  }
  /**
   * Get engine status
   */
  getStatus() {
    /* istanbul ignore next */
    cov_1btwfziq8q().f[28]++;
    cov_1btwfziq8q().s[198]++;
    return {
      isRunning: this.isRunning,
      historySize: this.sentimentHistory.length,
      lastUpdate: this.sentimentHistory.length > 0 ?
      /* istanbul ignore next */
      (cov_1btwfziq8q().b[48][0]++, this.sentimentHistory[this.sentimentHistory.length - 1].timestamp) :
      /* istanbul ignore next */
      (cov_1btwfziq8q().b[48][1]++, null),
      sources: {
        twitter: true,
        // Would check actual monitor status
        reddit: true,
        news: true
      }
    };
  }
}
