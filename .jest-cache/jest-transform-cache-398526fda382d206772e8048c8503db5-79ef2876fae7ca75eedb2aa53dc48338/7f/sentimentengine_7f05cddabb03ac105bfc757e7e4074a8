  async generateSentimentReport() {
    /* istanbul ignore next */
    cov_1btwfziq8q().f[18]++;
    const weightedSentiment =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[138]++, await this.performAggregatedAnalysis());
    const trends =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[139]++, this.analyzeSentimentTrends());
    const alerts =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[140]++, await this.generateSentimentAlerts(weightedSentiment));
    const recommendations =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[141]++, this.generateRecommendations(weightedSentiment, trends));
    /* istanbul ignore next */
    cov_1btwfziq8q().s[142]++;
    return {
      overall: weightedSentiment,
      trends,
      sources: {
        twitter:
        /* istanbul ignore next */
        (cov_1btwfziq8q().b[27][0]++, weightedSentiment.sources.find(s => {
          /* istanbul ignore next */
          cov_1btwfziq8q().f[19]++;
          cov_1btwfziq8q().s[143]++;
          return s.source === 'twitter';
        })) ||
        /* istanbul ignore next */
        (cov_1btwfziq8q().b[27][1]++, this.createEmptySentimentScore('twitter')),
        reddit:
        /* istanbul ignore next */
        (cov_1btwfziq8q().b[28][0]++, weightedSentiment.sources.find(s => {
          /* istanbul ignore next */
          cov_1btwfziq8q().f[20]++;
          cov_1btwfziq8q().s[144]++;
          return s.source === 'reddit';
        })) ||
        /* istanbul ignore next */
        (cov_1btwfziq8q().b[28][1]++, this.createEmptySentimentScore('reddit')),
        news:
        /* istanbul ignore next */
        (cov_1btwfziq8q().b[29][0]++, weightedSentiment.sources.find(s => {
          /* istanbul ignore next */
          cov_1btwfziq8q().f[21]++;
          cov_1btwfziq8q().s[145]++;
          return s.source === 'news';
        })) ||
        /* istanbul ignore next */
        (cov_1btwfziq8q().b[29][1]++, this.createEmptySentimentScore('news'))
      },
      alerts,
      recommendations
    };
  }
  /**
   * Generate sentiment-based alerts
   */
  async generateSentimentAlerts(sentiment) {
    /* istanbul ignore next */
    cov_1btwfziq8q().f[22]++;
    const alerts =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[146]++, []);
    const thresholds =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[147]++, this.config.thresholds);
    // Extreme sentiment alerts
    /* istanbul ignore next */
    cov_1btwfziq8q().s[148]++;
    if (sentiment.aggregatedScore > thresholds.extremePositive) {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[30][0]++;
      cov_1btwfziq8q().s[149]++;
      alerts.push({
        id: `extreme_positive_${Date.now()}`,
        type: 'EXTREME_POSITIVE',
        severity: 'HIGH',
        message: `Extremely positive market sentiment: ${sentiment.aggregatedScore.toFixed(1)}`,
        timestamp: new Date(),
        source: 'aggregated',
        data: sentiment
      });
    } else {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[30][1]++;
      cov_1btwfziq8q().s[150]++;
      if (sentiment.aggregatedScore < thresholds.extremeNegative) {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[31][0]++;
        cov_1btwfziq8q().s[151]++;
        alerts.push({
          id: `extreme_negative_${Date.now()}`,
          type: 'EXTREME_NEGATIVE',
          severity: 'HIGH',
          message: `Extremely negative market sentiment: ${sentiment.aggregatedScore.toFixed(1)}`,
          timestamp: new Date(),
          source: 'aggregated',
          data: sentiment
        });
      } else
      /* istanbul ignore next */
      {
        cov_1btwfziq8q().b[31][1]++;
      }
    }
    // Rapid change alerts
    /* istanbul ignore next */
    cov_1btwfziq8q().s[152]++;
    if (this.sentimentHistory.length >= 2) {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[32][0]++;
      const previous =
      /* istanbul ignore next */
      (cov_1btwfziq8q().s[153]++, this.sentimentHistory[this.sentimentHistory.length - 2]);
      const change =
      /* istanbul ignore next */
      (cov_1btwfziq8q().s[154]++, sentiment.aggregatedScore - previous.aggregatedScore);
      /* istanbul ignore next */
      cov_1btwfziq8q().s[155]++;
      if (Math.abs(change) > thresholds.rapidChange) {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[33][0]++;
        cov_1btwfziq8q().s[156]++;
        alerts.push({
          id: `rapid_change_${Date.now()}`,
          type: 'RAPID_CHANGE',
          severity: 'MEDIUM',
          message: `Rapid sentiment change: ${change > 0 ?
          /* istanbul ignore next */
          (cov_1btwfziq8q().b[34][0]++, '+') :
          /* istanbul ignore next */
          (cov_1btwfziq8q().b[34][1]++, '')}${change.toFixed(1)} points`,
          timestamp: new Date(),
          source: 'aggregated',
          data: {
            current: sentiment,
            previous,
            change
          }
        });
      } else
      /* istanbul ignore next */
      {
        cov_1btwfziq8q().b[33][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1btwfziq8q().b[32][1]++;
    }
    cov_1btwfziq8q().s[157]++;
    return alerts;
  }
  /**
   * Generate trading recommendations based on sentiment
   */
  generateRecommendations(sentiment, trends) {
    /* istanbul ignore next */
    cov_1btwfziq8q().f[23]++;
    const recommendations =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[158]++, []);
    const score =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[159]++, sentiment.aggregatedScore);
    const confidence =
    /* istanbul ignore next */
    (cov_1btwfziq8q().s[160]++, sentiment.confidence);
    // High confidence recommendations
    /* istanbul ignore next */
    cov_1btwfziq8q().s[161]++;
    if (confidence > 70) {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[35][0]++;
      cov_1btwfziq8q().s[162]++;
      if (score > 50) {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[36][0]++;
        cov_1btwfziq8q().s[163]++;
        recommendations.push('Strong bullish sentiment detected - Consider increasing position sizes');
        /* istanbul ignore next */
        cov_1btwfziq8q().s[164]++;
        recommendations.push('High positive sentiment may indicate good entry opportunities');
      } else {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[36][1]++;
        cov_1btwfziq8q().s[165]++;
        if (score < -50) {
          /* istanbul ignore next */
          cov_1btwfziq8q().b[37][0]++;
          cov_1btwfziq8q().s[166]++;
          recommendations.push('Strong bearish sentiment detected - Consider reducing position sizes');
          /* istanbul ignore next */
          cov_1btwfziq8q().s[167]++;
          recommendations.push('High negative sentiment may indicate oversold conditions');
        } else
        /* istanbul ignore next */
        {
          cov_1btwfziq8q().b[37][1]++;
        }
      }
    } else
    /* istanbul ignore next */
    {
      cov_1btwfziq8q().b[35][1]++;
    }
    // Trend-based recommendations
    cov_1btwfziq8q().s[168]++;
    for (const trend of trends) {
      /* istanbul ignore next */
      cov_1btwfziq8q().s[169]++;
      if (trend.strength > 60) {
        /* istanbul ignore next */
        cov_1btwfziq8q().b[38][0]++;
        cov_1btwfziq8q().s[170]++;
        if (trend.direction === 'BULLISH') {
          /* istanbul ignore next */
          cov_1btwfziq8q().b[39][0]++;
          cov_1btwfziq8q().s[171]++;
          recommendations.push(`Strong bullish trend detected - Momentum may continue`);
        } else {
          /* istanbul ignore next */
          cov_1btwfziq8q().b[39][1]++;
          cov_1btwfziq8q().s[172]++;
          if (trend.direction === 'BEARISH') {
            /* istanbul ignore next */
            cov_1btwfziq8q().b[40][0]++;
            cov_1btwfziq8q().s[173]++;
            recommendations.push(`Strong bearish trend detected - Consider defensive positioning`);
          } else
          /* istanbul ignore next */
          {
            cov_1btwfziq8q().b[40][1]++;
          }
        }
      } else
      /* istanbul ignore next */
      {
        cov_1btwfziq8q().b[38][1]++;
      }
    }
    // Contrarian recommendations for extreme sentiment
    /* istanbul ignore next */
    cov_1btwfziq8q().s[174]++;
    if (Math.abs(score) > 80) {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[41][0]++;
      cov_1btwfziq8q().s[175]++;
      recommendations.push('Extreme sentiment levels may indicate potential reversal opportunities');
    } else
    /* istanbul ignore next */
    {
      cov_1btwfziq8q().b[41][1]++;
    }
    // Low confidence warnings
    cov_1btwfziq8q().s[176]++;
    if (confidence < 30) {
      /* istanbul ignore next */
      cov_1btwfziq8q().b[42][0]++;
      cov_1btwfziq8q().s[177]++;
      recommendations.push('Low sentiment confidence - Rely more on technical analysis');
    } else
    /* istanbul ignore next */
    {
      cov_1btwfziq8q().b[42][1]++;
    }
    cov_1btwfziq8q().s[178]++;
    return recommendations;
  }
  /**
   * Get current aggregated sentiment
