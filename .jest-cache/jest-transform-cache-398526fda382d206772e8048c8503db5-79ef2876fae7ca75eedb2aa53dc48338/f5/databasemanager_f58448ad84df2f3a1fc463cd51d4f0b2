      throw new Error('Database not initialized');
    } else
    /* istanbul ignore next */
    {
      cov_s6xzxixiw().b[6][1]++;
    }
    const startTime =
    /* istanbul ignore next */
    (cov_s6xzxixiw().s[53]++, Date.now());
    const client =
    /* istanbul ignore next */
    (cov_s6xzxixiw().s[54]++, await this.pool.connect());
