      /* istanbul ignore next */
      (cov_nzrg0jhkv().s[241]++, this.performGracefulShutdown());
      const timeoutPromise =
      /* istanbul ignore next */
      (cov_nzrg0jhkv().s[242]++, new Promise((_, reject) => {
