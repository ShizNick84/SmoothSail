  }
  /**
   * Process risk alerts based on assessment
   */
  async processRiskAlerts(assessment) {
    /* istanbul ignore next */
    cov_21z85bubi5().f[14]++;
    cov_21z85bubi5().s[145]++;
    // Create alerts based on risk level changes
    if (
    /* istanbul ignore next */
    (cov_21z85bubi5().b[30][0]++, assessment.riskLevel === 'CRITICAL') &&
    /* istanbul ignore next */
    (cov_21z85bubi5().b[30][1]++, !this.hasActiveAlert('CRITICAL_RISK'))) {
      /* istanbul ignore next */
      cov_21z85bubi5().b[29][0]++;
      cov_21z85bubi5().s[146]++;
      await this.createAlert('CRITICAL', 'CRITICAL_RISK', 'Portfolio risk level is CRITICAL - immediate action required');
    } else
    /* istanbul ignore next */
    {
      cov_21z85bubi5().b[29][1]++;
    }
    cov_21z85bubi5().s[147]++;
    if (
    /* istanbul ignore next */
    (cov_21z85bubi5().b[32][0]++, assessment.riskLevel === 'HIGH') &&
    /* istanbul ignore next */
    (cov_21z85bubi5().b[32][1]++, !this.hasActiveAlert('HIGH_RISK'))) {
      /* istanbul ignore next */
      cov_21z85bubi5().b[31][0]++;
      cov_21z85bubi5().s[148]++;
      await this.createAlert('HIGH', 'HIGH_RISK', 'Portfolio risk level is HIGH - review positions');
    } else
    /* istanbul ignore next */
    {
