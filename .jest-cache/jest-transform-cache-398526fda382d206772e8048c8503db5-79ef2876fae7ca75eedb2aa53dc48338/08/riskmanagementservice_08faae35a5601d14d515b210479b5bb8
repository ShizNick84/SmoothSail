  /**
   * Check and manage circuit breaker
   */
  async checkCircuitBreaker(riskScore, portfolioStatus) {
    /* istanbul ignore next */
    cov_21z85bubi5().f[9]++;
    const dailyLossPercent =
    /* istanbul ignore next */
    (cov_21z85bubi5().s[118]++, Math.abs(portfolioStatus.dailyPnL / portfolioStatus.accountBalance) * 100);
    // Check if circuit breaker should be triggered
    /* istanbul ignore next */
    cov_21z85bubi5().s[119]++;
    if (
    /* istanbul ignore next */
    (cov_21z85bubi5().b[24][0]++, !this.circuitBreakerStatus.active) &&
    /* istanbul ignore next */
    (cov_21z85bubi5().b[24][1]++, dailyLossPercent >= this.config.circuitBreakerThreshold)) {
      /* istanbul ignore next */
      cov_21z85bubi5().b[23][0]++;
      cov_21z85bubi5().s[120]++;
      await this.triggerCircuitBreaker(`Daily loss ${dailyLossPercent.toFixed(2)}% exceeds threshold ${this.config.circuitBreakerThreshold}%`);
    } else
    /* istanbul ignore next */
    {
      cov_21z85bubi5().b[23][1]++;
    }
    // Check if circuit breaker should be released
    cov_21z85bubi5().s[121]++;
    if (
    /* istanbul ignore next */
    (cov_21z85bubi5().b[26][0]++, this.circuitBreakerStatus.active) &&
    /* istanbul ignore next */
    (cov_21z85bubi5().b[26][1]++, this.circuitBreakerStatus.cooldownEndsAt)) {
      /* istanbul ignore next */
      cov_21z85bubi5().b[25][0]++;
      cov_21z85bubi5().s[122]++;
      if (new Date() > this.circuitBreakerStatus.cooldownEndsAt) {
        /* istanbul ignore next */
        cov_21z85bubi5().b[27][0]++;
        cov_21z85bubi5().s[123]++;
        await this.releaseCircuitBreaker();
      } else
      /* istanbul ignore next */
      {
        cov_21z85bubi5().b[27][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_21z85bubi5().b[25][1]++;
    }
  }
  /**
   * Trigger circuit breaker
   */
  async triggerCircuitBreaker(reason) {
    /* istanbul ignore next */
    cov_21z85bubi5().f[10]++;
    const now =
    /* istanbul ignore next */
    (cov_21z85bubi5().s[124]++, new Date());
    const cooldownEndsAt =
    /* istanbul ignore next */
    (cov_21z85bubi5().s[125]++, new Date(now.getTime() + this.config.coolingOffPeriod * 60 * 1000));
    /* istanbul ignore next */
    cov_21z85bubi5().s[126]++;
    this.circuitBreakerStatus = {
      active: true,
      triggeredAt: now,
      reason,
      cooldownEndsAt,
      triggerCount: this.circuitBreakerStatus.triggerCount + 1
    };
    /* istanbul ignore next */
    cov_21z85bubi5().s[127]++;
    logger.error(' CIRCUIT BREAKER TRIGGERED', {
      reason,
      cooldownEndsAt: cooldownEndsAt.toISOString(),
      triggerCount: this.circuitBreakerStatus.triggerCount
    });
    // Create critical alert
    /* istanbul ignore next */
    cov_21z85bubi5().s[128]++;
    await this.createAlert('CRITICAL', 'CIRCUIT_BREAKER_TRIGGERED', `Circuit breaker triggered: ${reason}`, {
      reason,
      cooldownEndsAt,
      triggerCount: this.circuitBreakerStatus.triggerCount
    });
    // Auto-close positions if enabled
    /* istanbul ignore next */
    cov_21z85bubi5().s[129]++;
    if (this.config.autoClosePositions) {
      /* istanbul ignore next */
      cov_21z85bubi5().b[28][0]++;
      cov_21z85bubi5().s[130]++;
      await this.emergencyClosePositions();
    } else
    /* istanbul ignore next */
    {
      cov_21z85bubi5().b[28][1]++;
    }
    // Audit log
    cov_21z85bubi5().s[131]++;
    await audit_service_1.auditService.createAuditEntry({
      auditId: `circuit_breaker_${Date.now()}`,
      eventType: 'CIRCUIT_BREAKER_TRIGGERED',
      actor: 'SYSTEM',
      resource: 'RISK_MANAGEMENT_SERVICE',
      action: 'TRIGGER_CIRCUIT_BREAKER',
      result: 'SUCCESS',
      timestamp: new Date(),
      auditData: {
        reason,
        cooldownEndsAt,
        triggerCount: this.circuitBreakerStatus.triggerCount
      }
    });
  }
  /**
