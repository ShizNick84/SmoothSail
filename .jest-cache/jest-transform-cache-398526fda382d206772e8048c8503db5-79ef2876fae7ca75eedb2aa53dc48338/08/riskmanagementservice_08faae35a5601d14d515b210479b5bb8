    }
    cov_21z85bubi5().s[149]++;
    if (
    /* istanbul ignore next */
    (cov_21z85bubi5().b[34][0]++, assessment.portfolioHeat > this.config.maxPortfolioHeat) &&
    /* istanbul ignore next */
    (cov_21z85bubi5().b[34][1]++, !this.hasActiveAlert('PORTFOLIO_HEAT'))) {
      /* istanbul ignore next */
      cov_21z85bubi5().b[33][0]++;
      cov_21z85bubi5().s[150]++;
