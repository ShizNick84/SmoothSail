      /* istanbul ignore next */
      (cov_21z85bubi5().b[35][1]++, !alert.acknowledged);
    });
  }
  /**
   * Start risk monitoring
   */
  startRiskMonitoring() {
    /* istanbul ignore next */
    cov_21z85bubi5().f[17]++;
    cov_21z85bubi5().s[153]++;
    if (this.monitoringInterval) {
      /* istanbul ignore next */
      cov_21z85bubi5().b[36][0]++;
      cov_21z85bubi5().s[154]++;
      clearInterval(this.monitoringInterval);
    } else
    /* istanbul ignore next */
    {
      cov_21z85bubi5().b[36][1]++;
    }
    cov_21z85bubi5().s[155]++;
    this.monitoringInterval = setInterval(async () => {
      /* istanbul ignore next */
      cov_21z85bubi5().f[18]++;
      cov_21z85bubi5().s[156]++;
      try {
        // Perform periodic risk assessment
        const marketCondition =
        /* istanbul ignore next */
        (cov_21z85bubi5().s[157]++, {
          volatility: 'MEDIUM',
          trend: 'SIDEWAYS',
          liquidity: 'HIGH',
          correlation: 0.5,
          fearGreedIndex: 50
        });
        /* istanbul ignore next */
        cov_21z85bubi5().s[158]++;
        await this.assessPortfolioRisk(marketCondition);
      } catch (error) {
        /* istanbul ignore next */
        cov_21z85bubi5().s[159]++;
        logger.error('Risk monitoring error:', error);
      }
    }, this.config.monitoringInterval * 1000);
    /* istanbul ignore next */
    cov_21z85bubi5().s[160]++;
    logger.info(`Risk monitoring started with ${this.config.monitoringInterval}s interval`);
  }
