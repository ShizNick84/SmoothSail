  /**
   * Get risk management statistics
   */
  getRiskStatistics() {
    /* istanbul ignore next */
    cov_21z85bubi5().f[24]++;
    const alertCounts =
    /* istanbul ignore next */
    (cov_21z85bubi5().s[180]++, Array.from(this.activeAlerts.values()).reduce((acc, alert) => {
      /* istanbul ignore next */
      cov_21z85bubi5().f[25]++;
      cov_21z85bubi5().s[181]++;
      acc[alert.level] = (
      /* istanbul ignore next */
      (cov_21z85bubi5().b[41][0]++, acc[alert.level]) ||
      /* istanbul ignore next */
      (cov_21z85bubi5().b[41][1]++, 0)) + 1;
      /* istanbul ignore next */
      cov_21z85bubi5().s[182]++;
      return acc;
    }, {}));
    const riskLevelDistribution =
    /* istanbul ignore next */
    (cov_21z85bubi5().s[183]++, this.riskHistory.reduce((acc, assessment) => {
      /* istanbul ignore next */
      cov_21z85bubi5().f[26]++;
      cov_21z85bubi5().s[184]++;
      acc[assessment.riskLevel] = (
      /* istanbul ignore next */
      (cov_21z85bubi5().b[42][0]++, acc[assessment.riskLevel]) ||
      /* istanbul ignore next */
      (cov_21z85bubi5().b[42][1]++, 0)) + 1;
      /* istanbul ignore next */
      cov_21z85bubi5().s[185]++;
      return acc;
    }, {}));
    const averageRiskScore =
    /* istanbul ignore next */
    (cov_21z85bubi5().s[186]++, this.riskHistory.length > 0 ?
    /* istanbul ignore next */
    (cov_21z85bubi5().b[43][0]++, this.riskHistory.reduce((sum, assessment) => {
      /* istanbul ignore next */
      cov_21z85bubi5().f[27]++;
      cov_21z85bubi5().s[187]++;
      return sum + assessment.overallRiskScore;
    }, 0) / this.riskHistory.length) :
    /* istanbul ignore next */
    (cov_21z85bubi5().b[43][1]++, 0));
    /* istanbul ignore next */
    cov_21z85bubi5().s[188]++;
    return {
      totalAssessments: this.riskHistory.length,
      circuitBreakerTriggers: this.circuitBreakerStatus.triggerCount,
      averageRiskScore,
      alertCounts,
      riskLevelDistribution
    };
  }
