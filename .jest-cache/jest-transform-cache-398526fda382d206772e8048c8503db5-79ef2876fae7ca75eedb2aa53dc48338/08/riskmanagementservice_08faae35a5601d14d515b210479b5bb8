    } else
    /* istanbul ignore next */
    {
      cov_21z85bubi5().b[33][1]++;
    }
  }
  /**
   * Check if alert type is already active
   */
  hasActiveAlert(type) {
    /* istanbul ignore next */
    cov_21z85bubi5().f[15]++;
    cov_21z85bubi5().s[151]++;
    return Array.from(this.activeAlerts.values()).some(alert => {
