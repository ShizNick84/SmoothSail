    });
  }
  /**
   * Get capital preservation statistics
   */
  getCapitalPreservationStats() {
    /* istanbul ignore next */
    cov_2i2u7rvvp().f[27]++;
    const completedDrawdowns =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[135]++, this.drawdownPeriods.filter(d => {
      /* istanbul ignore next */
      cov_2i2u7rvvp().f[28]++;
      cov_2i2u7rvvp().s[136]++;
      return d.endDate !== null;
    }));
    const averageDrawdownDuration =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[137]++, completedDrawdowns.length > 0 ?
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[38][0]++, completedDrawdowns.reduce((sum, d) => {
      /* istanbul ignore next */
      cov_2i2u7rvvp().f[29]++;
      cov_2i2u7rvvp().s[138]++;
      return sum + d.durationDays;
