    // Critical drawdown
    cov_2i2u7rvvp().s[119]++;
    if (drawdownStatus.currentDrawdown >= this.config.criticalDrawdownThreshold) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[33][0]++;
      cov_2i2u7rvvp().s[120]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[33][1]++;
    }
    // Daily loss limit exceeded
    cov_2i2u7rvvp().s[121]++;
    if (lossLimits.daily.remaining <= 0) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[34][0]++;
      cov_2i2u7rvvp().s[122]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[34][1]++;
    }
    cov_2i2u7rvvp().s[123]++;
    return true;
  }
  /**
   * Calculate recommended position size adjustment
   */
  calculatePositionSizeAdjustment(basePositionSize, drawdownStatus) {
    /* istanbul ignore next */
