      /* istanbul ignore next */
      cov_2i2u7rvvp().b[40][1]++;
      cov_2i2u7rvvp().s[147]++;
      if (this.tradingHalted) {
        /* istanbul ignore next */
        cov_2i2u7rvvp().b[41][0]++;
        cov_2i2u7rvvp().s[148]++;
        currentStatus = 'HALTED';
      } else
      /* istanbul ignore next */
      {
        cov_2i2u7rvvp().b[41][1]++;
      }
    }
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[149]++;
    return {
      totalDrawdownPeriods: this.drawdownPeriods.length,
      averageDrawdownDuration,
