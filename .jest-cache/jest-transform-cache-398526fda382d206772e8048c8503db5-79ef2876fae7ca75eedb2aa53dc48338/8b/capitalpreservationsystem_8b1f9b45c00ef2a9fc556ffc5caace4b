    /* istanbul ignore next */
    cov_2i2u7rvvp().f[17]++;
    const actions =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[93]++, []);
    // Critical drawdown - emergency stop
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[94]++;
