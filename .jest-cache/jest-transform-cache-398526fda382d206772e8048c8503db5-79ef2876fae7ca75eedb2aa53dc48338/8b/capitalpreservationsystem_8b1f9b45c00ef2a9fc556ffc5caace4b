  /**
   * Determine if trading should be allowed
   */
  shouldAllowTrading(drawdownStatus, lossLimits) {
    /* istanbul ignore next */
    cov_2i2u7rvvp().f[23]++;
    cov_2i2u7rvvp().s[117]++;
    // Trading halted by emergency measures
    if (
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[32][0]++, this.tradingHalted) ||
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[32][1]++, this.emergencyModeActive)) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[31][0]++;
      cov_2i2u7rvvp().s[118]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[31][1]++;
