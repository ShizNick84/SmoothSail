    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[38][1]++, 0));
    const maxHistoricalDrawdown =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[139]++, Math.max(...this.drawdownPeriods.map(d => {
      /* istanbul ignore next */
      cov_2i2u7rvvp().f[30]++;
      cov_2i2u7rvvp().s[140]++;
      return d.maxDrawdownPercent;
    }), 0));
    const emergencyActivations =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[141]++, this.drawdownPeriods.filter(d => {
      /* istanbul ignore next */
      cov_2i2u7rvvp().f[31]++;
      cov_2i2u7rvvp().s[142]++;
      return d.emergencyMeasuresActivated;
    }).length);
    const recoveryRate =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[143]++, completedDrawdowns.length > 0 ?
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[39][0]++, completedDrawdowns.length / this.drawdownPeriods.length * 100) :
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[39][1]++, 100));
    let currentStatus =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[144]++, 'NORMAL');
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[145]++;
    if (this.emergencyModeActive) {
      /* istanbul ignore next */
