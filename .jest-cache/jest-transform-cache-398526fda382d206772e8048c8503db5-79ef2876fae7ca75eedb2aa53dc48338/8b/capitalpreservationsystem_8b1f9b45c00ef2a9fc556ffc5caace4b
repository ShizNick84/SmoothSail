    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[36][1]++;
    }
    // Recovery conditions:
    // 1. Drawdown below recovery threshold
    // 2. Recovery progress above threshold
    cov_2i2u7rvvp().s[130]++;
    return /* istanbul ignore next */(cov_2i2u7rvvp().b[37][0]++, drawdownStatus.currentDrawdown < this.config.recoveryThreshold) &&
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[37][1]++, drawdownStatus.recoveryProgress > 80);
  }
  /**
   * Resume normal operations after recovery
