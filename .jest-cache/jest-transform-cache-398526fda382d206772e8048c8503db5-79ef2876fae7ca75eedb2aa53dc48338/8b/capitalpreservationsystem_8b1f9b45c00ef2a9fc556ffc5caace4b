      /* istanbul ignore next */
      (cov_2i2u7rvvp().s[89]++, [...btcPositions, ...ethPositions].reduce((sum, p) => {
        /* istanbul ignore next */
        cov_2i2u7rvvp().f[16]++;
        cov_2i2u7rvvp().s[90]++;
        return sum + Math.abs(p.size * p.currentPrice);
      }, 0));
      /* istanbul ignore next */
      cov_2i2u7rvvp().s[91]++;
      return correlatedExposure / totalExposure;
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[22][1]++;
    }
    cov_2i2u7rvvp().s[92]++;
    return 0;
  }
  /**
