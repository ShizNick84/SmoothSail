cov_7n4m2jwp1().s[199]++;
exports.defaultHealthMonitorConfig = {
  checkInterval: 30000,
  // 30 seconds
  checkTimeout: 10000,
  // 10 seconds
  failureThreshold: 3,
  // 3 consecutive failures
  recoveryThreshold: 2,
  // 2 consecutive successes
  autoRecovery: true,
  recoveryCooldown: 300000,
  // 5 minutes
  historyRetention: 100,
  // Keep last 100 health checks
  enableAlerts: true,
  alertThresholds: {
    cpu: 80,
    // 80% CPU usage
    memory: 85,
    // 85% memory usage
    disk: 90,
    // 90% disk usage
    responseTime: 5000 // 5 second response time
  }
};
