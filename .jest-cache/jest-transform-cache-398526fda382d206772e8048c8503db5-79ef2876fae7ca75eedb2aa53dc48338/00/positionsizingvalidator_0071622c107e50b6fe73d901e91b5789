    const mean =
    /* istanbul ignore next */
    (cov_7jbod461c().s[276]++, negativeReturns.reduce((sum, ret) => {
      /* istanbul ignore next */
      cov_7jbod461c().f[21]++;
      cov_7jbod461c().s[277]++;
      return sum + ret;
    }, 0) / negativeReturns.length);
    const variance =
    /* istanbul ignore next */
    (cov_7jbod461c().s[278]++, negativeReturns.reduce((sum, ret) => {
      /* istanbul ignore next */
      cov_7jbod461c().f[22]++;
      cov_7jbod461c().s[279]++;
      return sum + Math.pow(ret - mean, 2);
    }, 0) / negativeReturns.length);
    /* istanbul ignore next */
    cov_7jbod461c().s[280]++;
    return Math.sqrt(variance);
  }
