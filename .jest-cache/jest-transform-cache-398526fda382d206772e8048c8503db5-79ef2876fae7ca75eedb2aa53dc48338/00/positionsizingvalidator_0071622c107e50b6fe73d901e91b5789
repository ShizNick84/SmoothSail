        /* istanbul ignore next */
        cov_7jbod461c().b[0][0]++;
        cov_7jbod461c().s[33]++;
        errors.push(...basicValidation.errors);
        /* istanbul ignore next */
        cov_7jbod461c().s[34]++;
        isValid = false;
      } else
      /* istanbul ignore next */
      {
        cov_7jbod461c().b[0][1]++;
      }
      cov_7jbod461c().s[35]++;
      warnings.push(...basicValidation.warnings);
      /* istanbul ignore next */
      cov_7jbod461c().s[36]++;
      riskScore += basicValidation.riskScore;
      // 2. Portfolio-level validation
      const portfolioValidation =
      /* istanbul ignore next */
      (cov_7jbod461c().s[37]++, this.performPortfolioValidation(request, positionValue));
      /* istanbul ignore next */
