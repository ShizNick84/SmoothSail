  /**
   * Calculate portfolio volatility
   */
  calculateVolatility() {
    /* istanbul ignore next */
    cov_7jbod461c().f[13]++;
    cov_7jbod461c().s[248]++;
    if (this.historicalReturns.length < 2) {
      /* istanbul ignore next */
      cov_7jbod461c().b[51][0]++;
      cov_7jbod461c().s[249]++;
      return 0.2;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[51][1]++;
    } // Default volatility
    const mean =
    /* istanbul ignore next */
    (cov_7jbod461c().s[250]++, this.historicalReturns.reduce((sum, ret) => {
      /* istanbul ignore next */
      cov_7jbod461c().f[14]++;
      cov_7jbod461c().s[251]++;
      return sum + ret;
    }, 0) / this.historicalReturns.length);
    const variance =
    /* istanbul ignore next */
    (cov_7jbod461c().s[252]++, this.historicalReturns.reduce((sum, ret) => {
      /* istanbul ignore next */
      cov_7jbod461c().f[15]++;
      cov_7jbod461c().s[253]++;
      return sum + Math.pow(ret - mean, 2);
    }, 0) / (this.historicalReturns.length - 1));
    /* istanbul ignore next */
    cov_7jbod461c().s[254]++;
    return Math.sqrt(variance);
  }
  /**
   * Calculate maximum drawdown
   */
  calculateMaxDrawdown() {
    /* istanbul ignore next */
    cov_7jbod461c().f[16]++;
    cov_7jbod461c().s[255]++;
    if (this.historicalReturns.length === 0) {
      /* istanbul ignore next */
      cov_7jbod461c().b[52][0]++;
      cov_7jbod461c().s[256]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[52][1]++;
    }
    let peak =
    /* istanbul ignore next */
    (cov_7jbod461c().s[257]++, 0);
    let maxDrawdown =
    /* istanbul ignore next */
    (cov_7jbod461c().s[258]++, 0);
    let cumulative =
    /* istanbul ignore next */
    (cov_7jbod461c().s[259]++, 0);
    /* istanbul ignore next */
    cov_7jbod461c().s[260]++;
    for (const ret of this.historicalReturns) {
      /* istanbul ignore next */
      cov_7jbod461c().s[261]++;
      cumulative += ret;
      /* istanbul ignore next */
      cov_7jbod461c().s[262]++;
      peak = Math.max(peak, cumulative);
      const drawdown =
      /* istanbul ignore next */
      (cov_7jbod461c().s[263]++, peak - cumulative);
      /* istanbul ignore next */
      cov_7jbod461c().s[264]++;
      maxDrawdown = Math.max(maxDrawdown, drawdown);
    }
    /* istanbul ignore next */
    cov_7jbod461c().s[265]++;
    return maxDrawdown;
  }
