    const negativeReturns =
    /* istanbul ignore next */
    (cov_7jbod461c().s[272]++, this.historicalReturns.filter(ret => {
      /* istanbul ignore next */
      cov_7jbod461c().f[20]++;
      cov_7jbod461c().s[273]++;
      return ret < 0;
    }));
    /* istanbul ignore next */
    cov_7jbod461c().s[274]++;
    if (negativeReturns.length === 0) {
      /* istanbul ignore next */
      cov_7jbod461c().b[55][0]++;
      cov_7jbod461c().s[275]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[55][1]++;
