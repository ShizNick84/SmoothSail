  /**
   * Perform basic validation checks
   */
  performBasicValidation(request, positionValue) {
    /* istanbul ignore next */
    cov_7jbod461c().f[3]++;
    const errors =
    /* istanbul ignore next */
    (cov_7jbod461c().s[72]++, []);
    const warnings =
    /* istanbul ignore next */
    (cov_7jbod461c().s[73]++, []);
    let riskScore =
    /* istanbul ignore next */
    (cov_7jbod461c().s[74]++, 0);
    // Check minimum values
    /* istanbul ignore next */
    cov_7jbod461c().s[75]++;
    if (request.quantity <= 0) {
      /* istanbul ignore next */
      cov_7jbod461c().b[8][0]++;
      cov_7jbod461c().s[76]++;
      errors.push('Position quantity must be greater than zero');
      /* istanbul ignore next */
      cov_7jbod461c().s[77]++;
      riskScore += 50;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[8][1]++;
    }
    cov_7jbod461c().s[78]++;
    if (request.price <= 0) {
      /* istanbul ignore next */
      cov_7jbod461c().b[9][0]++;
      cov_7jbod461c().s[79]++;
      errors.push('Position price must be greater than zero');
      /* istanbul ignore next */
      cov_7jbod461c().s[80]++;
      riskScore += 50;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[9][1]++;
    }
    // Check maximum position size in USD
    cov_7jbod461c().s[81]++;
    if (positionValue > this.config.maxPositionSizeUSD) {
      /* istanbul ignore next */
      cov_7jbod461c().b[10][0]++;
      cov_7jbod461c().s[82]++;
      errors.push(`Position value $${positionValue.toFixed(2)} exceeds maximum allowed $${this.config.maxPositionSizeUSD}`);
      /* istanbul ignore next */
      cov_7jbod461c().s[83]++;
      riskScore += 30;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[10][1]++;
    }
    // Check maximum position size as percentage of portfolio
    const portfolioPercent =
    /* istanbul ignore next */
    (cov_7jbod461c().s[84]++, positionValue / this.accountBalance * 100);
    /* istanbul ignore next */
    cov_7jbod461c().s[85]++;
    if (portfolioPercent > this.config.maxPositionSizePercent) {
      /* istanbul ignore next */
      cov_7jbod461c().b[11][0]++;
      cov_7jbod461c().s[86]++;
      errors.push(`Position size ${portfolioPercent.toFixed(2)}% exceeds maximum allowed ${this.config.maxPositionSizePercent}%`);
      /* istanbul ignore next */
      cov_7jbod461c().s[87]++;
      riskScore += 25;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[11][1]++;
    }
    // Check account balance
    cov_7jbod461c().s[88]++;
    if (this.accountBalance < this.config.minAccountBalance) {
      /* istanbul ignore next */
      cov_7jbod461c().b[12][0]++;
      cov_7jbod461c().s[89]++;
      errors.push(`Account balance $${this.accountBalance} below minimum required $${this.config.minAccountBalance}`);
      /* istanbul ignore next */
      cov_7jbod461c().s[90]++;
      riskScore += 40;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[12][1]++;
    }
    // Check leverage
    cov_7jbod461c().s[91]++;
    if (
    /* istanbul ignore next */
    (cov_7jbod461c().b[14][0]++, request.leverage) &&
    /* istanbul ignore next */
    (cov_7jbod461c().b[14][1]++, request.leverage > 10)) {
      /* istanbul ignore next */
      cov_7jbod461c().b[13][0]++;
      cov_7jbod461c().s[92]++;
      warnings.push(`High leverage ${request.leverage}x detected - increased risk`);
      /* istanbul ignore next */
      cov_7jbod461c().s[93]++;
      riskScore += 15;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[13][1]++;
    }
    cov_7jbod461c().s[94]++;
    return {
      isValid: errors.length === 0,
      errors,
      warnings,
      riskScore
    };
  }
  /**
   * Perform portfolio-level validation
   */
  performPortfolioValidation(request, positionValue) {
    /* istanbul ignore next */
    cov_7jbod461c().f[4]++;
    const errors =
    /* istanbul ignore next */
    (cov_7jbod461c().s[95]++, []);
    const warnings =
    /* istanbul ignore next */
    (cov_7jbod461c().s[96]++, []);
    let riskScore =
    /* istanbul ignore next */
    (cov_7jbod461c().s[97]++, 0);
    // Check maximum number of open positions
    /* istanbul ignore next */
    cov_7jbod461c().s[98]++;
    if (this.currentPositions.size >= this.config.maxOpenPositions) {
      /* istanbul ignore next */
      cov_7jbod461c().b[15][0]++;
      cov_7jbod461c().s[99]++;
      errors.push(`Maximum number of open positions (${this.config.maxOpenPositions}) reached`);
      /* istanbul ignore next */
      cov_7jbod461c().s[100]++;
      riskScore += 20;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[15][1]++;
    }
    // Check if position already exists for this symbol
    const existingPosition =
    /* istanbul ignore next */
    (cov_7jbod461c().s[101]++, this.currentPositions.get(request.symbol));
    /* istanbul ignore next */
    cov_7jbod461c().s[102]++;
    if (existingPosition) {
      /* istanbul ignore next */
      cov_7jbod461c().b[16][0]++;
      const totalQuantity =
      /* istanbul ignore next */
      (cov_7jbod461c().s[103]++, existingPosition.quantity + request.quantity);
      const totalValue =
      /* istanbul ignore next */
      (cov_7jbod461c().s[104]++, totalQuantity * request.price);
      /* istanbul ignore next */
      cov_7jbod461c().s[105]++;
      if (totalValue > this.config.maxPositionSizeUSD) {
        /* istanbul ignore next */
        cov_7jbod461c().b[17][0]++;
        cov_7jbod461c().s[106]++;
        errors.push(`Combined position value would exceed maximum allowed size`);
        /* istanbul ignore next */
        cov_7jbod461c().s[107]++;
        riskScore += 25;
      } else
      /* istanbul ignore next */
      {
        cov_7jbod461c().b[17][1]++;
      }
      cov_7jbod461c().s[108]++;
      warnings.push(`Adding to existing position in ${request.symbol}`);
      /* istanbul ignore next */
      cov_7jbod461c().s[109]++;
      riskScore += 5;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[16][1]++;
    }
    // Check portfolio concentration
    const totalPortfolioValue =
    /* istanbul ignore next */
    (cov_7jbod461c().s[110]++, Array.from(this.currentPositions.values()).reduce((sum, pos) => {
      /* istanbul ignore next */
      cov_7jbod461c().f[5]++;
      cov_7jbod461c().s[111]++;
      return sum + pos.quantity * pos.currentPrice;
    }, 0) + positionValue);
    const concentrationPercent =
    /* istanbul ignore next */
    (cov_7jbod461c().s[112]++, positionValue / totalPortfolioValue * 100);
    /* istanbul ignore next */
    cov_7jbod461c().s[113]++;
    if (concentrationPercent > 25) {
      /* istanbul ignore next */
      cov_7jbod461c().b[18][0]++;
      cov_7jbod461c().s[114]++;
      warnings.push(`High concentration risk: ${concentrationPercent.toFixed(2)}% in single position`);
      /* istanbul ignore next */
      cov_7jbod461c().s[115]++;
      riskScore += 10;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[18][1]++;
    }
    cov_7jbod461c().s[116]++;
    return {
      isValid: errors.length === 0,
      errors,
      warnings,
      riskScore
    };
  }
  /**
   * Perform risk limit validation
   */
  performRiskLimitValidation(request, positionValue) {
    /* istanbul ignore next */
    cov_7jbod461c().f[6]++;
    const errors =
    /* istanbul ignore next */
    (cov_7jbod461c().s[117]++, []);
    const warnings =
    /* istanbul ignore next */
    (cov_7jbod461c().s[118]++, []);
    let riskScore =
    /* istanbul ignore next */
    (cov_7jbod461c().s[119]++, 0);
    // Check daily loss limits
    const dailyLossPercent =
    /* istanbul ignore next */
    (cov_7jbod461c().s[120]++, Math.abs(this.dailyPnL) / this.accountBalance * 100);
    /* istanbul ignore next */
    cov_7jbod461c().s[121]++;
    if (dailyLossPercent >= this.config.maxDailyLossPercent) {
      /* istanbul ignore next */
      cov_7jbod461c().b[19][0]++;
      cov_7jbod461c().s[122]++;
      errors.push(`Daily loss limit ${this.config.maxDailyLossPercent}% reached (${dailyLossPercent.toFixed(2)}%)`);
      /* istanbul ignore next */
      cov_7jbod461c().s[123]++;
      riskScore += 40;
    } else {
      /* istanbul ignore next */
      cov_7jbod461c().b[19][1]++;
      cov_7jbod461c().s[124]++;
      if (dailyLossPercent >= this.config.maxDailyLossPercent * 0.8) {
        /* istanbul ignore next */
        cov_7jbod461c().b[20][0]++;
        cov_7jbod461c().s[125]++;
        warnings.push(`Approaching daily loss limit: ${dailyLossPercent.toFixed(2)}%`);
        /* istanbul ignore next */
        cov_7jbod461c().s[126]++;
        riskScore += 15;
      } else
      /* istanbul ignore next */
      {
        cov_7jbod461c().b[20][1]++;
      }
    }
    // Check weekly loss limits
    const weeklyLossPercent =
    /* istanbul ignore next */
    (cov_7jbod461c().s[127]++, Math.abs(this.weeklyPnL) / this.accountBalance * 100);
    /* istanbul ignore next */
    cov_7jbod461c().s[128]++;
    if (weeklyLossPercent >= this.config.maxWeeklyLossPercent) {
      /* istanbul ignore next */
      cov_7jbod461c().b[21][0]++;
      cov_7jbod461c().s[129]++;
      errors.push(`Weekly loss limit ${this.config.maxWeeklyLossPercent}% reached (${weeklyLossPercent.toFixed(2)}%)`);
      /* istanbul ignore next */
      cov_7jbod461c().s[130]++;
      riskScore += 35;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[21][1]++;
    }
    // Check monthly loss limits
    const monthlyLossPercent =
    /* istanbul ignore next */
    (cov_7jbod461c().s[131]++, Math.abs(this.monthlyPnL) / this.accountBalance * 100);
    /* istanbul ignore next */
    cov_7jbod461c().s[132]++;
    if (monthlyLossPercent >= this.config.maxMonthlyLossPercent) {
      /* istanbul ignore next */
      cov_7jbod461c().b[22][0]++;
      cov_7jbod461c().s[133]++;
      errors.push(`Monthly loss limit ${this.config.maxMonthlyLossPercent}% reached (${monthlyLossPercent.toFixed(2)}%)`);
      /* istanbul ignore next */
      cov_7jbod461c().s[134]++;
      riskScore += 30;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[22][1]++;
    }
    // Check stop loss configuration
    cov_7jbod461c().s[135]++;
    if (!request.stopLoss) {
      /* istanbul ignore next */
      cov_7jbod461c().b[23][0]++;
      cov_7jbod461c().s[136]++;
      warnings.push('No stop loss specified - increased risk');
      /* istanbul ignore next */
      cov_7jbod461c().s[137]++;
      riskScore += 10;
    } else {
      /* istanbul ignore next */
      cov_7jbod461c().b[23][1]++;
      const stopLossPercent =
      /* istanbul ignore next */
      (cov_7jbod461c().s[138]++, Math.abs((request.stopLoss - request.price) / request.price) * 100);
      /* istanbul ignore next */
      cov_7jbod461c().s[139]++;
      if (stopLossPercent > 10) {
        /* istanbul ignore next */
        cov_7jbod461c().b[24][0]++;
        cov_7jbod461c().s[140]++;
        warnings.push(`Wide stop loss ${stopLossPercent.toFixed(2)}% - high risk per trade`);
        /* istanbul ignore next */
        cov_7jbod461c().s[141]++;
        riskScore += 8;
      } else
      /* istanbul ignore next */
      {
        cov_7jbod461c().b[24][1]++;
      }
    }
    /* istanbul ignore next */
    cov_7jbod461c().s[142]++;
    return {
      isValid: errors.length === 0,
      errors,
      warnings,
      riskScore
    };
  }
  /**
   * Perform market condition validation
   */
  performMarketConditionValidation(request, marketCondition, positionValue) {
    /* istanbul ignore next */
    cov_7jbod461c().f[7]++;
    const errors =
    /* istanbul ignore next */
    (cov_7jbod461c().s[143]++, []);
    const warnings =
    /* istanbul ignore next */
    (cov_7jbod461c().s[144]++, []);
    let riskScore =
    /* istanbul ignore next */
    (cov_7jbod461c().s[145]++, 0);
    // Adjust limits based on market volatility
    let volatilityMultiplier =
    /* istanbul ignore next */
    (cov_7jbod461c().s[146]++, 1);
    /* istanbul ignore next */
    cov_7jbod461c().s[147]++;
    switch (marketCondition.volatility) {
      case 'EXTREME':
        /* istanbul ignore next */
        cov_7jbod461c().b[25][0]++;
        cov_7jbod461c().s[148]++;
        volatilityMultiplier = 0.3;
        /* istanbul ignore next */
        cov_7jbod461c().s[149]++;
        warnings.push('Extreme market volatility detected - reduced position sizing recommended');
        /* istanbul ignore next */
        cov_7jbod461c().s[150]++;
        riskScore += 25;
        /* istanbul ignore next */
        cov_7jbod461c().s[151]++;
        break;
      case 'HIGH':
        /* istanbul ignore next */
        cov_7jbod461c().b[25][1]++;
        cov_7jbod461c().s[152]++;
        volatilityMultiplier = 0.5;
        /* istanbul ignore next */
        cov_7jbod461c().s[153]++;
        warnings.push('High market volatility detected - consider smaller position sizes');
        /* istanbul ignore next */
        cov_7jbod461c().s[154]++;
        riskScore += 15;
        /* istanbul ignore next */
        cov_7jbod461c().s[155]++;
        break;
      case 'MEDIUM':
        /* istanbul ignore next */
        cov_7jbod461c().b[25][2]++;
        cov_7jbod461c().s[156]++;
        volatilityMultiplier = 0.8;
        /* istanbul ignore next */
        cov_7jbod461c().s[157]++;
        riskScore += 5;
        /* istanbul ignore next */
        cov_7jbod461c().s[158]++;
        break;
      case 'LOW':
        /* istanbul ignore next */
        cov_7jbod461c().b[25][3]++;
        cov_7jbod461c().s[159]++;
        volatilityMultiplier = 1.0;
        /* istanbul ignore next */
        cov_7jbod461c().s[160]++;
        break;
    }
    // Check adjusted position size
    const adjustedMaxSize =
    /* istanbul ignore next */
    (cov_7jbod461c().s[161]++, this.config.maxPositionSizeUSD * volatilityMultiplier);
    /* istanbul ignore next */
    cov_7jbod461c().s[162]++;
    if (positionValue > adjustedMaxSize) {
      /* istanbul ignore next */
      cov_7jbod461c().b[26][0]++;
      cov_7jbod461c().s[163]++;
      errors.push(`Position size exceeds volatility-adjusted limit: $${adjustedMaxSize.toFixed(2)}`);
      /* istanbul ignore next */
      cov_7jbod461c().s[164]++;
      riskScore += 20;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[26][1]++;
    }
    // Check liquidity conditions
    cov_7jbod461c().s[165]++;
    if (marketCondition.liquidity === 'LOW') {
      /* istanbul ignore next */
      cov_7jbod461c().b[27][0]++;
      cov_7jbod461c().s[166]++;
      warnings.push('Low market liquidity - execution risk increased');
      /* istanbul ignore next */
      cov_7jbod461c().s[167]++;
      riskScore += 10;
    } else
    /* istanbul ignore next */
    {
      cov_7jbod461c().b[27][1]++;
    }
    // Check fear & greed index
    cov_7jbod461c().s[168]++;
    if (marketCondition.fearGreedIndex > 80) {
      /* istanbul ignore next */
      cov_7jbod461c().b[28][0]++;
      cov_7jbod461c().s[169]++;
      warnings.push('Extreme greed detected - consider reducing position sizes');
      /* istanbul ignore next */
      cov_7jbod461c().s[170]++;
      riskScore += 8;
    } else {
      /* istanbul ignore next */
      cov_7jbod461c().b[28][1]++;
      cov_7jbod461c().s[171]++;
