  }
  /**
   * Calculate downward deviation for Sortino ratio
   */
  calculateDownwardDeviation() {
    /* istanbul ignore next */
    cov_7jbod461c().f[19]++;
    cov_7jbod461c().s[270]++;
    if (this.historicalReturns.length === 0) {
      /* istanbul ignore next */
