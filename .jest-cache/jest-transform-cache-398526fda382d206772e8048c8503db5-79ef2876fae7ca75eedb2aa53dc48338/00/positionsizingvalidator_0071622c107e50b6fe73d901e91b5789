  }
  /**
   * Get current portfolio status
   */
  getPortfolioStatus() {
    /* istanbul ignore next */
    cov_7jbod461c().f[29]++;
    const totalValue =
    /* istanbul ignore next */
    (cov_7jbod461c().s[305]++, Array.from(this.currentPositions.values()).reduce((sum, pos) => {
      /* istanbul ignore next */
      cov_7jbod461c().f[30]++;
      cov_7jbod461c().s[306]++;
      return sum + pos.quantity * pos.currentPrice;
    }, 0));
    const riskMetrics =
    /* istanbul ignore next */
    (cov_7jbod461c().s[307]++, this.calculateRiskMetrics({
      symbol: 'PORTFOLIO',
      side: 'BUY',
      quantity: 1,
      price: totalValue,
      orderType: 'MARKET'
    }, totalValue));
    /* istanbul ignore next */
    cov_7jbod461c().s[308]++;
    return {
      totalPositions: this.currentPositions.size,
      totalValue,
      dailyPnL: this.dailyPnL,
      weeklyPnL: this.weeklyPnL,
      monthlyPnL: this.monthlyPnL,
      accountBalance: this.accountBalance,
      riskMetrics
    };
  }
  /**
   * Emergency position sizing override
   */
  async emergencyPositionSizing(enabled) {
    /* istanbul ignore next */
    cov_7jbod461c().f[31]++;
    cov_7jbod461c().s[309]++;
    if (enabled) {
      /* istanbul ignore next */
      cov_7jbod461c().b[66][0]++;
      cov_7jbod461c().s[310]++;
      // Reduce all position limits by 50%
      this.config.maxPositionSizePercent *= 0.5;
      /* istanbul ignore next */
      cov_7jbod461c().s[311]++;
      this.config.maxPositionSizeUSD *= 0.5;
      /* istanbul ignore next */
      cov_7jbod461c().s[312]++;
      this.config.maxOpenPositions = Math.floor(this.config.maxOpenPositions * 0.5);
      /* istanbul ignore next */
      cov_7jbod461c().s[313]++;
      logger.warn('🚨 Emergency position sizing activated - all limits reduced by 50%');
    } else {
      /* istanbul ignore next */
      cov_7jbod461c().b[66][1]++;
      cov_7jbod461c().s[314]++;
      // Restore original limits (would need to store originals)
      logger.info('✅ Emergency position sizing deactivated');
    }
    // Audit log
    /* istanbul ignore next */
    cov_7jbod461c().s[315]++;
    await audit_service_1.auditService.createAuditEntry({
      auditId: `emergency_sizing_${Date.now()}`,
      eventType: 'EMERGENCY_POSITION_SIZING',
      actor: 'SYSTEM',
      resource: 'POSITION_SIZING_VALIDATOR',
      action: enabled ?
      /* istanbul ignore next */
      (cov_7jbod461c().b[67][0]++, 'ACTIVATE') :
      /* istanbul ignore next */
      (cov_7jbod461c().b[67][1]++, 'DEACTIVATE'),
      result: 'SUCCESS',
      timestamp: new Date(),
      auditData: {
        enabled,
        config: this.config
      }
    });
  }
