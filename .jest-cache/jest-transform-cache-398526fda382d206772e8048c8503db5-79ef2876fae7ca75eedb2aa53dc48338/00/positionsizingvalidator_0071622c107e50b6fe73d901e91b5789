  /**
   * Update position after trade execution
   */
  async updatePosition(symbol, quantity, price, side) {
    /* istanbul ignore next */
    cov_7jbod461c().f[28]++;
    const existingPosition =
    /* istanbul ignore next */
    (cov_7jbod461c().s[295]++, this.currentPositions.get(symbol));
    /* istanbul ignore next */
    cov_7jbod461c().s[296]++;
    if (existingPosition) {
      /* istanbul ignore next */
      cov_7jbod461c().b[62][0]++;
      cov_7jbod461c().s[297]++;
      // Update existing position
      if (side === 'BUY') {
        /* istanbul ignore next */
        cov_7jbod461c().b[63][0]++;
        cov_7jbod461c().s[298]++;
        existingPosition.quantity += quantity;
      } else {
        /* istanbul ignore next */
        cov_7jbod461c().b[63][1]++;
        cov_7jbod461c().s[299]++;
        existingPosition.quantity -= quantity;
      }
      // Remove position if quantity becomes zero or negative
      /* istanbul ignore next */
      cov_7jbod461c().s[300]++;
      if (existingPosition.quantity <= 0) {
        /* istanbul ignore next */
        cov_7jbod461c().b[64][0]++;
        cov_7jbod461c().s[301]++;
        this.currentPositions.delete(symbol);
      } else
      /* istanbul ignore next */
      {
        cov_7jbod461c().b[64][1]++;
      }
    } else {
      /* istanbul ignore next */
      cov_7jbod461c().b[62][1]++;
      cov_7jbod461c().s[302]++;
      if (side === 'BUY') {
        /* istanbul ignore next */
        cov_7jbod461c().b[65][0]++;
        cov_7jbod461c().s[303]++;
        // Create new position
        this.currentPositions.set(symbol, {
          symbol,
          quantity,
          entryPrice: price,
          currentPrice: price,
          unrealizedPnL: 0,
          realizedPnL: 0,
          openTime: new Date()
        });
      } else
      /* istanbul ignore next */
      {
        cov_7jbod461c().b[65][1]++;
      }
    }
    // Audit log
    /* istanbul ignore next */
    cov_7jbod461c().s[304]++;
    await audit_service_1.auditService.createAuditEntry({
      auditId: `pos_update_${Date.now()}`,
      eventType: 'POSITION_UPDATE',
      actor: 'SYSTEM',
      resource: 'POSITION_SIZING_VALIDATOR',
      action: 'UPDATE_POSITION',
      result: 'SUCCESS',
