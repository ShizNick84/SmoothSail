   * Calculate trailing stop loss based on market conditions
   */
  calculateTrailingStopLoss(position, config, marketConditions) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[2]++;
    const currentPrice =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[17]++, position.currentPrice);
    let trailingDistance =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[18]++, config.trailingDistance);
    // Adjust trailing distance based on volatility
    /* istanbul ignore next */
    cov_wfaaug54p().s[19]++;
    if (config.volatilityAdjustment) {
      /* istanbul ignore next */
      cov_wfaaug54p().b[2][0]++;
      cov_wfaaug54p().s[20]++;
      trailingDistance = this.adjustTrailingDistanceForVolatility(trailingDistance, marketConditions.volatility, marketConditions.atr);
    } else
    /* istanbul ignore next */
    {
      cov_wfaaug54p().b[2][1]++;
    }
    // Calculate base trailing stop
    let trailingStop;
    /* istanbul ignore next */
    cov_wfaaug54p().s[21]++;
    if (position.type === 'LONG') {
      /* istanbul ignore next */
      cov_wfaaug54p().b[3][0]++;
      cov_wfaaug54p().s[22]++;
      trailingStop = currentPrice * (1 - trailingDistance / 100);
      // Consider support levels for long positions
      /* istanbul ignore next */
      cov_wfaaug54p().s[23]++;
      if (
      /* istanbul ignore next */
      (cov_wfaaug54p().b[5][0]++, marketConditions.supportLevel) &&
      /* istanbul ignore next */
      (cov_wfaaug54p().b[5][1]++, marketConditions.supportLevel > trailingStop)) {
        /* istanbul ignore next */
        cov_wfaaug54p().b[4][0]++;
        cov_wfaaug54p().s[24]++;
        trailingStop = Math.max(trailingStop, marketConditions.supportLevel * 0.995); // 0.5% below support
      } else
      /* istanbul ignore next */
      {
        cov_wfaaug54p().b[4][1]++;
      }
    } else {
      /* istanbul ignore next */
      cov_wfaaug54p().b[3][1]++;
      cov_wfaaug54p().s[25]++;
      trailingStop = currentPrice * (1 + trailingDistance / 100);
      // Consider resistance levels for short positions
      /* istanbul ignore next */
      cov_wfaaug54p().s[26]++;
      if (
      /* istanbul ignore next */
      (cov_wfaaug54p().b[7][0]++, marketConditions.resistanceLevel) &&
      /* istanbul ignore next */
      (cov_wfaaug54p().b[7][1]++, marketConditions.resistanceLevel < trailingStop)) {
        /* istanbul ignore next */
        cov_wfaaug54p().b[6][0]++;
        cov_wfaaug54p().s[27]++;
        trailingStop = Math.min(trailingStop, marketConditions.resistanceLevel * 1.005); // 0.5% above resistance
      } else
      /* istanbul ignore next */
      {
        cov_wfaaug54p().b[6][1]++;
      }
    }
    /* istanbul ignore next */
    cov_wfaaug54p().s[28]++;
    return trailingStop;
  }
  /**
   * Adjust trailing distance based on market volatility
   */
  adjustTrailingDistanceForVolatility(baseDistance, volatility, atr) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[3]++;
    // Increase trailing distance in high volatility to avoid premature stops
    const volatilityMultiplier =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[29]++, 1 + volatility * 0.5);
    // Use ATR to determine minimum trailing distance
    const atrBasedDistance =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[30]++, atr / 100 * 2); // 2x ATR as minimum distance
    const adjustedDistance =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[31]++, Math.max(baseDistance * volatilityMultiplier, atrBasedDistance));
    // Cap maximum trailing distance at 5%
    /* istanbul ignore next */
    cov_wfaaug54p().s[32]++;
    return Math.min(adjustedDistance, 5.0);
  }
  /**
   * Apply breakeven stop loss logic
   */
  applyBreakevenLogic(position, calculatedStopLoss, config, profitPercentage) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[4]++;
    const entryPrice =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[33]++, position.entryPrice);
    // Check if we should move to breakeven
    /* istanbul ignore next */
    cov_wfaaug54p().s[34]++;
    if (profitPercentage >= config.breakevenThreshold) {
      /* istanbul ignore next */
      cov_wfaaug54p().b[8][0]++;
      let breakevenStop;
      /* istanbul ignore next */
      cov_wfaaug54p().s[35]++;
      if (position.type === 'LONG') {
        /* istanbul ignore next */
        cov_wfaaug54p().b[9][0]++;
        cov_wfaaug54p().s[36]++;
        // For long positions, breakeven is slightly above entry
        breakevenStop = entryPrice * 1.001; // 0.1% above entry to cover fees
        // Use the higher of calculated trailing stop or breakeven
        const finalStop =
        /* istanbul ignore next */
        (cov_wfaaug54p().s[37]++, Math.max(calculatedStopLoss, breakevenStop));
        /* istanbul ignore next */
        cov_wfaaug54p().s[38]++;
        return {
          stopLoss: finalStop,
          breakevenActive: true,
          reason: finalStop === breakevenStop ?
          /* istanbul ignore next */
          (cov_wfaaug54p().b[10][0]++, `Breakeven stop activated at ${profitPercentage.toFixed(2)}% profit`) :
          /* istanbul ignore next */
          (cov_wfaaug54p().b[10][1]++, `Trailing stop above breakeven at ${profitPercentage.toFixed(2)}% profit`)
        };
      } else {
        /* istanbul ignore next */
        cov_wfaaug54p().b[9][1]++;
        cov_wfaaug54p().s[39]++;
        // For short positions, breakeven is slightly below entry
        breakevenStop = entryPrice * 0.999; // 0.1% below entry to cover fees
        // Use the lower of calculated trailing stop or breakeven
        const finalStop =
        /* istanbul ignore next */
        (cov_wfaaug54p().s[40]++, Math.min(calculatedStopLoss, breakevenStop));
        /* istanbul ignore next */
        cov_wfaaug54p().s[41]++;
        return {
          stopLoss: finalStop,
          breakevenActive: true,
          reason: finalStop === breakevenStop ?
          /* istanbul ignore next */
          (cov_wfaaug54p().b[11][0]++, `Breakeven stop activated at ${profitPercentage.toFixed(2)}% profit`) :
          /* istanbul ignore next */
          (cov_wfaaug54p().b[11][1]++, `Trailing stop below breakeven at ${profitPercentage.toFixed(2)}% profit`)
        };
      }
    } else
    /* istanbul ignore next */
    {
      cov_wfaaug54p().b[8][1]++;
    }
    cov_wfaaug54p().s[42]++;
    return {
      stopLoss: calculatedStopLoss,
      breakevenActive: false,
      reason: `Trailing stop updated, ${profitPercentage.toFixed(2)}% profit`
    };
  }
  /**
   * Calculate current profit percentage
   */
  calculateProfitPercentage(position) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[5]++;
    const entryPrice =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[43]++, position.entryPrice);
    const currentPrice =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[44]++, position.currentPrice);
    /* istanbul ignore next */
    cov_wfaaug54p().s[45]++;
    if (position.type === 'LONG') {
      /* istanbul ignore next */
      cov_wfaaug54p().b[12][0]++;
      cov_wfaaug54p().s[46]++;
      return (currentPrice - entryPrice) / entryPrice * 100;
    } else {
      /* istanbul ignore next */
      cov_wfaaug54p().b[12][1]++;
      cov_wfaaug54p().s[47]++;
      return (entryPrice - currentPrice) / entryPrice * 100;
    }
  }
  /**
   * Determine if stop loss should be updated
   */
  shouldUpdateStopLoss(position, currentStopLoss, newStopLoss) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[6]++;
    cov_wfaaug54p().s[48]++;
    if (position.type === 'LONG') {
      /* istanbul ignore next */
      cov_wfaaug54p().b[13][0]++;
      cov_wfaaug54p().s[49]++;
      // For long positions, only move stop loss up
      return newStopLoss > currentStopLoss;
    } else {
      /* istanbul ignore next */
      cov_wfaaug54p().b[13][1]++;
      cov_wfaaug54p().s[50]++;
      // For short positions, only move stop loss down
      return newStopLoss < currentStopLoss;
    }
  }
  /**
   * Calculate actual trailing distance percentage
   */
  calculateActualTrailingDistance(currentPrice, stopLoss) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[7]++;
    cov_wfaaug54p().s[51]++;
    return Math.abs((currentPrice - stopLoss) / currentPrice) * 100;
  }
  /**
   * Record stop loss update for audit trail
   */
  recordStopLossUpdate(positionId, previousStopLoss, newStopLoss, reason) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[8]++;
    const update =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[52]++, {
      positionId,
      previousStopLoss,
      newStopLoss,
      reason,
      timestamp: new Date()
    });
    /* istanbul ignore next */
    cov_wfaaug54p().s[53]++;
    if (!this.trailingStopUpdates.has(positionId)) {
      /* istanbul ignore next */
      cov_wfaaug54p().b[14][0]++;
      cov_wfaaug54p().s[54]++;
      this.trailingStopUpdates.set(positionId, []);
    } else
    /* istanbul ignore next */
    {
      cov_wfaaug54p().b[14][1]++;
    }
    cov_wfaaug54p().s[55]++;
    this.trailingStopUpdates.get(positionId).push(update);
    // Keep only last 100 updates per position
    const updates =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[56]++, this.trailingStopUpdates.get(positionId));
    /* istanbul ignore next */
    cov_wfaaug54p().s[57]++;
    if (updates.length > 100) {
      /* istanbul ignore next */
      cov_wfaaug54p().b[15][0]++;
      cov_wfaaug54p().s[58]++;
      updates.splice(0, updates.length - 100);
    } else
    /* istanbul ignore next */
    {
      cov_wfaaug54p().b[15][1]++;
    }
  }
  /**
   * Get trailing stop update history for a position
   */
  getTrailingStopHistory(positionId) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[9]++;
    cov_wfaaug54p().s[59]++;
    return /* istanbul ignore next */(cov_wfaaug54p().b[16][0]++, this.trailingStopUpdates.get(positionId)) ||
    /* istanbul ignore next */
    (cov_wfaaug54p().b[16][1]++, []);
  }
  /**
   * Calculate optimal initial stop loss
   */
  calculateInitialStopLoss(entryPrice, positionType, config, marketConditions) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[10]++;
    let initialStopDistance =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[60]++, config.initialStopLoss);
    // Adjust for volatility
    /* istanbul ignore next */
    cov_wfaaug54p().s[61]++;
    if (config.volatilityAdjustment) {
      /* istanbul ignore next */
      cov_wfaaug54p().b[17][0]++;
      const volatilityAdjustment =
      /* istanbul ignore next */
      (cov_wfaaug54p().s[62]++, marketConditions.volatility * 0.5);
      /* istanbul ignore next */
      cov_wfaaug54p().s[63]++;
      initialStopDistance = Math.max(initialStopDistance, initialStopDistance * (1 + volatilityAdjustment));
    } else
    /* istanbul ignore next */
    {
      cov_wfaaug54p().b[17][1]++;
    }
    // Use ATR for minimum stop distance
    const atrBasedStop =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[64]++, marketConditions.atr / entryPrice * 100 * 1.5); // 1.5x ATR
    /* istanbul ignore next */
    cov_wfaaug54p().s[65]++;
    initialStopDistance = Math.max(initialStopDistance, atrBasedStop);
    /* istanbul ignore next */
    cov_wfaaug54p().s[66]++;
    if (positionType === 'LONG') {
      /* istanbul ignore next */
      cov_wfaaug54p().b[18][0]++;
      cov_wfaaug54p().s[67]++;
      return entryPrice * (1 - initialStopDistance / 100);
    } else {
      /* istanbul ignore next */
      cov_wfaaug54p().b[18][1]++;
      cov_wfaaug54p().s[68]++;
      return entryPrice * (1 + initialStopDistance / 100);
    }
  }
  /**
   * Optimize stop loss based on current market conditions
   */
  optimizeStopLoss(position, config, marketConditions) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[11]++;
    const currentStopLoss =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[69]++, position.stopLoss);
    const currentPrice =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[70]++, position.currentPrice);
    // Check if current stop is too tight in high volatility
    /* istanbul ignore next */
    cov_wfaaug54p().s[71]++;
    if (marketConditions.volatility > 0.5) {
      /* istanbul ignore next */
      cov_wfaaug54p().b[19][0]++;
      const minDistance =
      /* istanbul ignore next */
      (cov_wfaaug54p().s[72]++, marketConditions.atr / currentPrice * 100 * 2);
      const currentDistance =
      /* istanbul ignore next */
      (cov_wfaaug54p().s[73]++, this.calculateActualTrailingDistance(currentPrice, currentStopLoss));
      /* istanbul ignore next */
      cov_wfaaug54p().s[74]++;
      if (currentDistance < minDistance) {
        /* istanbul ignore next */
        cov_wfaaug54p().b[20][0]++;
        const optimizedStop =
        /* istanbul ignore next */
        (cov_wfaaug54p().s[75]++, position.type === 'LONG' ?
        /* istanbul ignore next */
        (cov_wfaaug54p().b[21][0]++, currentPrice * (1 - minDistance / 100)) :
        /* istanbul ignore next */
        (cov_wfaaug54p().b[21][1]++, currentPrice * (1 + minDistance / 100)));
        /* istanbul ignore next */
        cov_wfaaug54p().s[76]++;
        return {
          optimizedStopLoss: optimizedStop,
          reason: `Stop loss widened due to high volatility (${(marketConditions.volatility * 100).toFixed(1)}%)`
        };
      } else
      /* istanbul ignore next */
      {
        cov_wfaaug54p().b[20][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_wfaaug54p().b[19][1]++;
    }
    // Check for support/resistance optimization
    cov_wfaaug54p().s[77]++;
    if (
    /* istanbul ignore next */
    (cov_wfaaug54p().b[23][0]++, position.type === 'LONG') &&
    /* istanbul ignore next */
    (cov_wfaaug54p().b[23][1]++, marketConditions.supportLevel)) {
      /* istanbul ignore next */
      cov_wfaaug54p().b[22][0]++;
      const supportBasedStop =
      /* istanbul ignore next */
      (cov_wfaaug54p().s[78]++, marketConditions.supportLevel * 0.995);
      /* istanbul ignore next */
      cov_wfaaug54p().s[79]++;
      if (supportBasedStop > currentStopLoss) {
        /* istanbul ignore next */
        cov_wfaaug54p().b[24][0]++;
        cov_wfaaug54p().s[80]++;
        return {
          optimizedStopLoss: supportBasedStop,
          reason: `Stop loss moved to support level at ${marketConditions.supportLevel}`
        };
      } else
      /* istanbul ignore next */
      {
        cov_wfaaug54p().b[24][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_wfaaug54p().b[22][1]++;
    }
    cov_wfaaug54p().s[81]++;
    if (
    /* istanbul ignore next */
    (cov_wfaaug54p().b[26][0]++, position.type === 'SHORT') &&
    /* istanbul ignore next */
    (cov_wfaaug54p().b[26][1]++, marketConditions.resistanceLevel)) {
      /* istanbul ignore next */
      cov_wfaaug54p().b[25][0]++;
      const resistanceBasedStop =
      /* istanbul ignore next */
      (cov_wfaaug54p().s[82]++, marketConditions.resistanceLevel * 1.005);
      /* istanbul ignore next */
      cov_wfaaug54p().s[83]++;
      if (resistanceBasedStop < currentStopLoss) {
        /* istanbul ignore next */
        cov_wfaaug54p().b[27][0]++;
        cov_wfaaug54p().s[84]++;
        return {
          optimizedStopLoss: resistanceBasedStop,
          reason: `Stop loss moved to resistance level at ${marketConditions.resistanceLevel}`
        };
      } else
      /* istanbul ignore next */
      {
        cov_wfaaug54p().b[27][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_wfaaug54p().b[25][1]++;
    }
    cov_wfaaug54p().s[85]++;
    return {
      optimizedStopLoss: currentStopLoss,
      reason: 'Current stop loss is optimal for market conditions'
    };
  }
  /**
   * Clear trailing stop history for a position
   */
  clearTrailingStopHistory(positionId) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[12]++;
    cov_wfaaug54p().s[86]++;
    this.trailingStopUpdates.delete(positionId);
  }
  /**
   * Get statistics for trailing stop performance
   */
  getTrailingStopStatistics(positionId) {
    /* istanbul ignore next */
    cov_wfaaug54p().f[13]++;
    const updates =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[87]++, this.getTrailingStopHistory(positionId));
    /* istanbul ignore next */
    cov_wfaaug54p().s[88]++;
    if (updates.length === 0) {
      /* istanbul ignore next */
      cov_wfaaug54p().b[28][0]++;
      cov_wfaaug54p().s[89]++;
      return {
        totalUpdates: 0,
        averageTrailingDistance: 0,
        maxTrailingDistance: 0,
        minTrailingDistance: 0,
        breakevenActivations: 0
      };
    } else
    /* istanbul ignore next */
    {
      cov_wfaaug54p().b[28][1]++;
    }
    const distances =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[90]++, updates.map(update => {
      /* istanbul ignore next */
      cov_wfaaug54p().f[14]++;
      cov_wfaaug54p().s[91]++;
      return Math.abs((update.newStopLoss - update.previousStopLoss) / update.previousStopLoss) * 100;
    }));
    const breakevenActivations =
    /* istanbul ignore next */
    (cov_wfaaug54p().s[92]++, updates.filter(update => {
      /* istanbul ignore next */
      cov_wfaaug54p().f[15]++;
      cov_wfaaug54p().s[93]++;
      return update.reason.includes('Breakeven');
    }).length);
    /* istanbul ignore next */
    cov_wfaaug54p().s[94]++;
    return {
      totalUpdates: updates.length,
      averageTrailingDistance: distances.reduce((sum, d) => {
        /* istanbul ignore next */
        cov_wfaaug54p().f[16]++;
        cov_wfaaug54p().s[95]++;
        return sum + d;
      }, 0) / distances.length,
      maxTrailingDistance: Math.max(...distances),
      minTrailingDistance: Math.min(...distances),
      breakevenActivations
    };
  }
}
/* istanbul ignore next */
cov_wfaaug54p().s[96]++;
exports.TrailingStopManager = TrailingStopManager;