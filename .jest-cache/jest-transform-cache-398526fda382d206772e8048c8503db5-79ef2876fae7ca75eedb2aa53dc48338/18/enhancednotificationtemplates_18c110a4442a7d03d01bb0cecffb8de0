  /**
   * Generate trade update template for stop loss approaching
   */
  static generateStopLossApproachingTemplate(context) {
    /* istanbul ignore next */
    cov_2ejtfkibdj().f[1]++;
    const {
      alert,
      position,
      marketData,
      calculations,
      systemMetrics
    } =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[12]++, context);
    /* istanbul ignore next */
    cov_2ejtfkibdj().s[13]++;
    if (
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[11][0]++, !position) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[11][1]++, !marketData)) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[10][0]++;
      cov_2ejtfkibdj().s[14]++;
      throw new Error('Position and market data required for stop loss template');
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[10][1]++;
    }
    const distancePercent =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[15]++, Math.abs((position.currentPrice - position.stopLoss) / position.currentPrice * 100));
    const urgencyEmoji =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[16]++, distancePercent <= 1 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[12][0]++, '🚨') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[12][1]++, distancePercent <= 3 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[13][0]++, '⚠️') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[13][1]++, '📊')));
    const pnlEmoji =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[17]++, position.unrealizedPnL >= 0 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[14][0]++, '💰') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[14][1]++, '📉'));
    const trendEmoji =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[18]++, this.getTrendEmoji(calculations.trendDirection));
    /* istanbul ignore next */
    cov_2ejtfkibdj().s[19]++;
    return `
${urgencyEmoji} <b>PRICE APPROACHING STOP LOSS</b> 🛑 <i>[Intel NUC]</i>

🎯 <b>Position Alert:</b>
• 💎 <b>Symbol:</b> ${position.symbol}
• 📉 <b>Current Price:</b> $${position.currentPrice.toFixed(4)}
• 🛑 <b>Stop Loss:</b> $${position.stopLoss.toFixed(4)}
• 📊 <b>Distance:</b> ${distancePercent.toFixed(2)}% (${Math.abs(position.currentPrice - position.stopLoss).toFixed(4)})

💰 <b>Position Details:</b>
• 📊 <b>Size:</b> ${position.size.toLocaleString()}
• 💵 <b>Entry Price:</b> $${position.entryPrice.toFixed(4)}
• ${pnlEmoji} <b>Unrealized P&L:</b> $${position.unrealizedPnL.toFixed(2)} (${position.unrealizedPnLPercent.toFixed(2)}%)
• ⏱️ <b>Duration:</b> ${this.formatDuration(position.duration)}
• 📈 <b>Max Profit:</b> $${position.maxProfit.toFixed(2)}
• 📉 <b>Max Drawdown:</b> $${position.maxDrawdown.toFixed(2)}

📊 <b>Real-Time Market Analysis:</b>
• 💵 <b>Current Price:</b> $${marketData.currentPrice.toFixed(4)}
• 📈 <b>24h Change:</b> ${marketData.priceChangePercent24h >= 0 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[15][0]++, '+') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[15][1]++, '')}${marketData.priceChangePercent24h.toFixed(2)}%
• 📊 <b>Volume:</b> ${this.formatVolume(marketData.volume24h)}
• ${trendEmoji} <b>Trend:</b> ${calculations.trendDirection}
• 🎯 <b>Support Level:</b> $${
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[16][0]++, marketData.technicalIndicators?.support.toFixed(4)) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[16][1]++, 'N/A')}

📈 <b>Technical Indicators:</b>
• 📊 <b>RSI:</b> ${
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[17][0]++, marketData.technicalIndicators?.rsi.toFixed(1)) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[17][1]++, 'N/A')}
• 📈 <b>EMA20:</b> $${
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[18][0]++, marketData.technicalIndicators?.ema20.toFixed(4)) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[18][1]++, 'N/A')}
• 📊 <b>Bollinger:</b> $${
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[19][0]++, marketData.technicalIndicators?.bollinger.lower.toFixed(4)) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[19][1]++, 'N/A')} - $${
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[20][0]++, marketData.technicalIndicators?.bollinger.upper.toFixed(4)) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[20][1]++, 'N/A')}

💡 <b>Recommendations:</b>
${distancePercent <= 1 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[21][0]++, '• 🚨 <b>URGENT:</b> Consider immediate action') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[21][1]++, '')}
${distancePercent <= 3 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[22][0]++, '• ⚠️ Monitor closely for potential exit') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[22][1]++, '')}
• 🔄 Consider adjusting stop loss if trend reverses
• 📊 Watch support level at $${
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[23][0]++, marketData.technicalIndicators?.support.toFixed(4)) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[23][1]++, 'N/A')}
• 📈 Monitor volume for confirmation

⏰ <i>${systemMetrics.timestamp.toLocaleString()}</i>
    `.trim();
  }
  /**
   * Generate trade closed profit template
   */
  static generateTradeClosedProfitTemplate(context) {
    /* istanbul ignore next */
    cov_2ejtfkibdj().f[2]++;
    const {
      alert,
      position,
      marketData,
      calculations,
      systemMetrics
    } =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[20]++, context);
    /* istanbul ignore next */
    cov_2ejtfkibdj().s[21]++;
    if (
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[25][0]++, !position) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[25][1]++, !marketData)) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[24][0]++;
      cov_2ejtfkibdj().s[22]++;
      throw new Error('Position and market data required for trade closed template');
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[24][1]++;
    }
    const profitEmoji =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[23]++, position.realizedPnL > 1000 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[26][0]++, '🚀') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[26][1]++, position.realizedPnL > 500 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[27][0]++, '💎') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[27][1]++, position.realizedPnL > 100 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[28][0]++, '💰') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[28][1]++, '📈'))));
    const returnPercent =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[24]++, (position.currentPrice - position.entryPrice) / position.entryPrice * 100);
    const performanceEmoji =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[25]++, returnPercent > 10 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[29][0]++, '🏆') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[29][1]++, returnPercent > 5 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[30][0]++, '🎯') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[30][1]++, '✅')));
    /* istanbul ignore next */
    cov_2ejtfkibdj().s[26]++;
    return `
${profitEmoji} <b>PROFIT TARGET HIT!</b> 🎯 <i>[Intel NUC]</i>

💰 <b>Trade Summary:</b>
• 💎 <b>Symbol:</b> ${position.symbol}
• ${performanceEmoji} <b>Realized P&L:</b> $${position.realizedPnL.toFixed(2)}
• 📊 <b>Return:</b> ${returnPercent >= 0 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[31][0]++, '+') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[31][1]++, '')}${returnPercent.toFixed(2)}%
• ⏱️ <b>Duration:</b> ${this.formatDuration(position.duration)}

📈 <b>Execution Details:</b>
• 💵 <b>Entry Price:</b> $${position.entryPrice.toFixed(4)}
• 🎯 <b>Exit Price:</b> $${position.currentPrice.toFixed(4)}
• 📊 <b>Quantity:</b> ${position.size.toLocaleString()}
• 💎 <b>Total Value:</b> $${(position.size * position.currentPrice).toLocaleString()}

📊 <b>Performance Metrics:</b>
• 🏆 <b>Max Profit:</b> $${position.maxProfit.toFixed(2)}
• 📉 <b>Max Drawdown:</b> $${position.maxDrawdown.toFixed(2)}
• 📈 <b>Profit Factor:</b> ${(
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[32][0]++, position.maxProfit / Math.abs(position.maxDrawdown)) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[32][1]++, 1)).toFixed(2)}
• ⚖️ <b>Risk/Reward:</b> 1:${calculations.riskReward.toFixed(2)}

📊 <b>Market Context at Exit:</b>
• 💵 <b>Market Price:</b> $${marketData.currentPrice.toFixed(4)}
• 📈 <b>24h Change:</b> ${marketData.priceChangePercent24h >= 0 ?
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[33][0]++, '+') :
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[33][1]++, '')}${marketData.priceChangePercent24h.toFixed(2)}%
• 📊 <b>Volume:</b> ${this.formatVolume(marketData.volume24h)}
• 🎯 <b>Resistance Hit:</b> $${
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[34][0]++, marketData.technicalIndicators?.resistance.toFixed(4)) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[34][1]++, 'N/A')}

