  static formatVolume(volume) {
    /* istanbul ignore next */
    cov_2ejtfkibdj().f[10]++;
    cov_2ejtfkibdj().s[83]++;
    if (volume >= 1000000000) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[61][0]++;
      cov_2ejtfkibdj().s[84]++;
      return `${(volume / 1000000000).toFixed(2)}B`;
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[61][1]++;
    }
    cov_2ejtfkibdj().s[85]++;
    if (volume >= 1000000) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[62][0]++;
      cov_2ejtfkibdj().s[86]++;
      return `${(volume / 1000000).toFixed(2)}M`;
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[62][1]++;
    }
    cov_2ejtfkibdj().s[87]++;
    if (volume >= 1000) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[63][0]++;
      cov_2ejtfkibdj().s[88]++;
      return `${(volume / 1000).toFixed(2)}K`;
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[63][1]++;
    }
    cov_2ejtfkibdj().s[89]++;
    return volume.toFixed(2);
  }
  static formatDuration(minutes) {
    /* istanbul ignore next */
    cov_2ejtfkibdj().f[11]++;
    const hours =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[90]++, Math.floor(minutes / 60));
    const mins =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[91]++, minutes % 60);
    const days =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[92]++, Math.floor(hours / 24));
    const remainingHours =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[93]++, hours % 24);
    /* istanbul ignore next */
    cov_2ejtfkibdj().s[94]++;
    if (days > 0) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[64][0]++;
      cov_2ejtfkibdj().s[95]++;
      return `${days}d ${remainingHours}h ${mins}m`;
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[64][1]++;
    }
    cov_2ejtfkibdj().s[96]++;
    if (hours > 0) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[65][0]++;
      cov_2ejtfkibdj().s[97]++;
      return `${hours}h ${mins}m`;
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[65][1]++;
    }
    cov_2ejtfkibdj().s[98]++;
    return `${mins}m`;
  }
}
