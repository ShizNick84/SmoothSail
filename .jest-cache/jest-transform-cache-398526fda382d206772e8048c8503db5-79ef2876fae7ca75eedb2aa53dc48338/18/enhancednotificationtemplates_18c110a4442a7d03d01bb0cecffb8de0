  static formatVolume(volume) {
    /* istanbul ignore next */
    cov_2ejtfkibdj().f[14]++;
    cov_2ejtfkibdj().s[112]++;
    if (volume >= 1000000000) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[87][0]++;
      cov_2ejtfkibdj().s[113]++;
      return `${(volume / 1000000000).toFixed(2)}B`;
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[87][1]++;
    }
    cov_2ejtfkibdj().s[114]++;
    if (volume >= 1000000) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[88][0]++;
      cov_2ejtfkibdj().s[115]++;
      return `${(volume / 1000000).toFixed(2)}M`;
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[88][1]++;
    }
    cov_2ejtfkibdj().s[116]++;
    if (volume >= 1000) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[89][0]++;
      cov_2ejtfkibdj().s[117]++;
      return `${(volume / 1000).toFixed(2)}K`;
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[89][1]++;
    }
    cov_2ejtfkibdj().s[118]++;
    return volume.toFixed(2);
  }
  static formatDuration(minutes) {
    /* istanbul ignore next */
    cov_2ejtfkibdj().f[15]++;
    const hours =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[119]++, Math.floor(minutes / 60));
    const mins =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[120]++, minutes % 60);
    const days =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[121]++, Math.floor(hours / 24));
    const remainingHours =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[122]++, hours % 24);
    /* istanbul ignore next */
    cov_2ejtfkibdj().s[123]++;
    if (days > 0) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[90][0]++;
      cov_2ejtfkibdj().s[124]++;
      return `${days}d ${remainingHours}h ${mins}m`;
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[90][1]++;
    }
    cov_2ejtfkibdj().s[125]++;
    if (hours > 0) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[91][0]++;
      cov_2ejtfkibdj().s[126]++;
      return `${hours}h ${mins}m`;
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[91][1]++;
    }
    cov_2ejtfkibdj().s[127]++;
    return `${mins}m`;
  }
}
