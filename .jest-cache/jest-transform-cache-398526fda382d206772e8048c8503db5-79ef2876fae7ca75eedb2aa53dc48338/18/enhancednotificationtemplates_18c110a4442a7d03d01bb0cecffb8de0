  /**
   * Generate trade closed profit email template
   */
  static generateTradeClosedProfitEmailTemplate(context) {
    /* istanbul ignore next */
    cov_2ejtfkibdj().f[13]++;
    const {
      alert,
      position,
      marketData,
      calculations,
      systemMetrics
    } =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[106]++, context);
    /* istanbul ignore next */
    cov_2ejtfkibdj().s[107]++;
    if (
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[79][0]++, !position) ||
    /* istanbul ignore next */
    (cov_2ejtfkibdj().b[79][1]++, !marketData)) {
      /* istanbul ignore next */
      cov_2ejtfkibdj().b[78][0]++;
      cov_2ejtfkibdj().s[108]++;
      throw new Error('Position and market data required for email template');
    } else
    /* istanbul ignore next */
    {
      cov_2ejtfkibdj().b[78][1]++;
    }
    const profitColor =
    /* istanbul ignore next */
    (cov_2ejtfkibdj().s[109]++, position.realizedPnL >= 0 ?
    /* istanbul ignore next */
