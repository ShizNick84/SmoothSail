      return setTimeout(resolve, 10000);
    });
    // Get current sentiment
    const currentSentiment =
    /* istanbul ignore next */
    (cov_n3vfm84i2().s[14]++, await sentimentEngine.getCurrentSentiment());
    /* istanbul ignore next */
    cov_n3vfm84i2().s[15]++;
    logger_1.logger.info('Current sentiment:', {
      score: currentSentiment.aggregatedScore,
      confidence: currentSentiment.confidence,
      sources: currentSentiment.sources.map(s => {
        /* istanbul ignore next */
        cov_n3vfm84i2().f[2]++;
        cov_n3vfm84i2().s[16]++;
        return {
          source: s.source,
          score: s.score,
          confidence: s.confidence,
          volume: s.volume
        };
      })
    });
    // Generate comprehensive report
    const report =
    /* istanbul ignore next */
    (cov_n3vfm84i2().s[17]++, await sentimentEngine.generateSentimentReport());
    /* istanbul ignore next */
    cov_n3vfm84i2().s[18]++;
    logger_1.logger.info('Sentiment report generated:', {
      overallScore: report.overall.aggregatedScore,
      trendsCount: report.trends.length,
      alertsCount: report.alerts.length,
      recommendationsCount: report.recommendations.length
    });
    // Example of position sizing adjustment based on sentiment
    const baseSizePercent =
    /* istanbul ignore next */
    (cov_n3vfm84i2().s[19]++, 2.0); // 2% risk per trade
    const adjustedSize =
    /* istanbul ignore next */
    (cov_n3vfm84i2().s[20]++, sentimentEngine.calculateSentimentPositionAdjustment(baseSizePercent));
    /* istanbul ignore next */
    cov_n3vfm84i2().s[21]++;
    logger_1.logger.info('Position sizing adjustment:', {
      baseSize: baseSizePercent,
      adjustedSize: adjustedSize,
      adjustment: ((adjustedSize / baseSizePercent - 1) * 100).toFixed(2) + '%'
    });
    // Get sentiment history
    const history =
    /* istanbul ignore next */
    (cov_n3vfm84i2().s[22]++, sentimentEngine.getSentimentHistory(1)); // Last 1 hour
    /* istanbul ignore next */
    cov_n3vfm84i2().s[23]++;
    logger_1.logger.info('Sentiment history:', {
      dataPoints: history.length,
      timeSpan: '1 hour',
      latestScore: history.length > 0 ?
      /* istanbul ignore next */
      (cov_n3vfm84i2().b[0][0]++, history[history.length - 1].aggregatedScore) :
      /* istanbul ignore next */
      (cov_n3vfm84i2().b[0][1]++, 'N/A')
    });
    // Get engine status
    const status =
    /* istanbul ignore next */
    (cov_n3vfm84i2().s[24]++, sentimentEngine.getStatus());
    /* istanbul ignore next */
    cov_n3vfm84i2().s[25]++;
    logger_1.logger.info('Engine status:', status);
    // Stop the engine
    /* istanbul ignore next */
    cov_n3vfm84i2().s[26]++;
    sentimentEngine.stopEngine();
    /* istanbul ignore next */
    cov_n3vfm84i2().s[27]++;
    logger_1.logger.info('Sentiment analysis example completed');
  } catch (error) {
    /* istanbul ignore next */
    cov_n3vfm84i2().s[28]++;
    logger_1.logger.error('Error in sentiment analysis example:', error);
    /* istanbul ignore next */
    cov_n3vfm84i2().s[29]++;
    throw error;
  }
}
/**
 * Example of integrating sentiment analysis with trading decisions
 */
function integrateSentimentWithTrading(sentimentScore, confidence, technicalSignal) {
  /* istanbul ignore next */
  cov_n3vfm84i2().f[3]++;
  let finalDecision =
  /* istanbul ignore next */
  (cov_n3vfm84i2().s[30]++, technicalSignal);
  let reasoning =
  /* istanbul ignore next */
  (cov_n3vfm84i2().s[31]++, `Technical signal: ${technicalSignal}`);
  let positionSizeMultiplier =
  /* istanbul ignore next */
  (cov_n3vfm84i2().s[32]++, 1.0);
  // Only consider sentiment if confidence is high enough
  /* istanbul ignore next */
  cov_n3vfm84i2().s[33]++;
  if (confidence > 60) {
    /* istanbul ignore next */
    cov_n3vfm84i2().b[1][0]++;
    cov_n3vfm84i2().s[34]++;
    if (
    /* istanbul ignore next */
    (cov_n3vfm84i2().b[3][0]++, sentimentScore > 50) &&
    /* istanbul ignore next */
    (cov_n3vfm84i2().b[3][1]++, technicalSignal === 'BUY')) {
      /* istanbul ignore next */
      cov_n3vfm84i2().b[2][0]++;
      cov_n3vfm84i2().s[35]++;
      // Strong positive sentiment reinforces buy signal
      positionSizeMultiplier = 1.3;
      /* istanbul ignore next */
      cov_n3vfm84i2().s[36]++;
      reasoning += `, Strong positive sentiment (${sentimentScore}) reinforces buy signal`;
    } else {
      /* istanbul ignore next */
      cov_n3vfm84i2().b[2][1]++;
      cov_n3vfm84i2().s[37]++;
      if (
      /* istanbul ignore next */
      (cov_n3vfm84i2().b[5][0]++, sentimentScore < -50) &&
      /* istanbul ignore next */
      (cov_n3vfm84i2().b[5][1]++, technicalSignal === 'SELL')) {
        /* istanbul ignore next */
        cov_n3vfm84i2().b[4][0]++;
        cov_n3vfm84i2().s[38]++;
        // Strong negative sentiment reinforces sell signal
        positionSizeMultiplier = 1.3;
        /* istanbul ignore next */
        cov_n3vfm84i2().s[39]++;
        reasoning += `, Strong negative sentiment (${sentimentScore}) reinforces sell signal`;
      } else {
        /* istanbul ignore next */
        cov_n3vfm84i2().b[4][1]++;
        cov_n3vfm84i2().s[40]++;
        if (
        /* istanbul ignore next */
        (cov_n3vfm84i2().b[7][0]++, sentimentScore > 70) &&
        /* istanbul ignore next */
        (cov_n3vfm84i2().b[7][1]++, technicalSignal === 'HOLD')) {
          /* istanbul ignore next */
          cov_n3vfm84i2().b[6][0]++;
          cov_n3vfm84i2().s[41]++;
          // Extremely positive sentiment might override hold
          finalDecision = 'BUY';
          /* istanbul ignore next */
          cov_n3vfm84i2().s[42]++;
          positionSizeMultiplier = 1.2;
          /* istanbul ignore next */
          cov_n3vfm84i2().s[43]++;
          reasoning += `, Extremely positive sentiment (${sentimentScore}) overrides hold signal`;
        } else {
          /* istanbul ignore next */
          cov_n3vfm84i2().b[6][1]++;
          cov_n3vfm84i2().s[44]++;
          if (
          /* istanbul ignore next */
          (cov_n3vfm84i2().b[9][0]++, sentimentScore < -70) &&
          /* istanbul ignore next */
          (cov_n3vfm84i2().b[9][1]++, technicalSignal === 'HOLD')) {
            /* istanbul ignore next */
            cov_n3vfm84i2().b[8][0]++;
            cov_n3vfm84i2().s[45]++;
            // Extremely negative sentiment might override hold
            finalDecision = 'SELL';
            /* istanbul ignore next */
            cov_n3vfm84i2().s[46]++;
            positionSizeMultiplier = 1.2;
            /* istanbul ignore next */
            cov_n3vfm84i2().s[47]++;
            reasoning += `, Extremely negative sentiment (${sentimentScore}) overrides hold signal`;
          } else {
            /* istanbul ignore next */
            cov_n3vfm84i2().b[8][1]++;
            cov_n3vfm84i2().s[48]++;
            if (Math.abs(sentimentScore) > 30) {
              /* istanbul ignore next */
              cov_n3vfm84i2().b[10][0]++;
              // Moderate sentiment affects position size
              const sentimentMultiplier =
              /* istanbul ignore next */
              (cov_n3vfm84i2().s[49]++, 1 + sentimentScore / 100 * 0.2);
              /* istanbul ignore next */
              cov_n3vfm84i2().s[50]++;
              positionSizeMultiplier = Math.max(0.5, Math.min(1.5, sentimentMultiplier));
              /* istanbul ignore next */
              cov_n3vfm84i2().s[51]++;
              reasoning += `, Sentiment (${sentimentScore}) adjusts position size`;
            } else
            /* istanbul ignore next */
            {
              cov_n3vfm84i2().b[10][1]++;
            }
          }
        }
      }
    }
  } else {
    /* istanbul ignore next */
    cov_n3vfm84i2().b[1][1]++;
    cov_n3vfm84i2().s[52]++;
    reasoning += `, Low sentiment confidence (${confidence}%) - relying on technical analysis`;
  }
  /* istanbul ignore next */
  cov_n3vfm84i2().s[53]++;
  return {
    finalDecision,
    reasoning,
    positionSizeMultiplier
  };
}
