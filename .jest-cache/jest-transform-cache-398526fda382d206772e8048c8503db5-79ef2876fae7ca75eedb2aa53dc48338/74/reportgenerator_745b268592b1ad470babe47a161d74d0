      } else {
        /* istanbul ignore next */
        cov_1fwcimt8ws().b[21][1]++;
        cov_1fwcimt8ws().s[110]++;
        assessment += '❌ **High** maximum drawdown (> 20%)\n';
      }
    }
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[111]++;
    if (risk.volatility < 20) {
      /* istanbul ignore next */
      cov_1fwcimt8ws().b[22][0]++;
      cov_1fwcimt8ws().s[112]++;
