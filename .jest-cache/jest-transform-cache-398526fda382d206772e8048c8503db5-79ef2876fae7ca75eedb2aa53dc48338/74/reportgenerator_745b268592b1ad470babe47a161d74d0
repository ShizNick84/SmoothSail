  static generateTradeDistribution(trades) {
    /* istanbul ignore next */
    cov_1fwcimt8ws().f[20]++;
    const closedTrades =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[117]++, trades.filter(t => {
      /* istanbul ignore next */
      cov_1fwcimt8ws().f[21]++;
      cov_1fwcimt8ws().s[118]++;
      return /* istanbul ignore next */(cov_1fwcimt8ws().b[24][0]++, t.status === 'CLOSED') &&
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[24][1]++, t.pnl !== undefined);
    }));
    const pnlRanges =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[119]++, [{
      min: -Infinity,
      max: -1000,
      label: '< -$1000'
    }, {
      min: -1000,
      max: -500,
      label: '-$1000 to -$500'
    }, {
      min: -500,
      max: -100,
      label: '-$500 to -$100'
    }, {
      min: -100,
      max: 0,
      label: '-$100 to $0'
    }, {
      min: 0,
      max: 100,
      label: '$0 to $100'
    }, {
      min: 100,
      max: 500,
      label: '$100 to $500'
    }, {
      min: 500,
      max: 1000,
      label: '$500 to $1000'
    }, {
      min: 1000,
      max: Infinity,
      label: '> $1000'
    }]);
    let distribution =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[120]++, '| P&L Range | Count | Percentage |\n|-----------|-------|------------|\n');
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[121]++;
    for (const range of pnlRanges) {
      const count =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[122]++, closedTrades.filter(t => {
        /* istanbul ignore next */
        cov_1fwcimt8ws().f[22]++;
        cov_1fwcimt8ws().s[123]++;
        return /* istanbul ignore next */(cov_1fwcimt8ws().b[25][0]++, t.pnl >= range.min) &&
        /* istanbul ignore next */
        (cov_1fwcimt8ws().b[25][1]++, t.pnl < range.max);
      }).length);
      const percentage =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[124]++, closedTrades.length > 0 ?
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[26][0]++, (count / closedTrades.length * 100).toFixed(1)) :
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[26][1]++, '0.0'));
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[125]++;
      distribution += `| ${range.label} | ${count} | ${percentage}% |\n`;
    }
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[126]++;
    return distribution;
  }
  static generateTopTrades(trades, type) {
    /* istanbul ignore next */
    cov_1fwcimt8ws().f[23]++;
    const closedTrades =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[127]++, trades.filter(t => {
      /* istanbul ignore next */
      cov_1fwcimt8ws().f[24]++;
      cov_1fwcimt8ws().s[128]++;
      return /* istanbul ignore next */(cov_1fwcimt8ws().b[27][0]++, t.status === 'CLOSED') &&
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[27][1]++, t.pnl !== undefined);
    }));
    const sortedTrades =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[129]++, closedTrades.sort((a, b) => {
      /* istanbul ignore next */
      cov_1fwcimt8ws().f[25]++;
      cov_1fwcimt8ws().s[130]++;
      return type === 'best' ?
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[28][0]++, b.pnl - a.pnl) :
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[28][1]++, a.pnl - b.pnl);
    }));
    const topTrades =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[131]++, sortedTrades.slice(0, 10));
    let table =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[132]++, '| Date | Symbol | Type | P&L | P&L % | Strategy |\n|------|--------|------|-----|-------|----------|\n');
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[133]++;
    for (const trade of topTrades) {
      const date =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[134]++, trade.entryTime.toISOString().split('T')[0]);
      const pnlClass =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[135]++, trade.pnl > 0 ?
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[29][0]++, '+') :
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[29][1]++, ''));
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[136]++;
      table += `| ${date} | ${trade.symbol} | ${trade.type} | ${pnlClass}$${trade.pnl.toFixed(2)} | ${pnlClass}${trade.pnlPercentage.toFixed(2)}% | ${trade.strategy} |\n`;
    }
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[137]++;
    return table;
  }
  static calculateYearlySummaries(monthlyReturns) {
    /* istanbul ignore next */
    cov_1fwcimt8ws().f[26]++;
    const yearlyData =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[138]++, {});
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[139]++;
    for (const month of monthlyReturns) {
      const year =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[140]++, month.year.toString());
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[141]++;
      if (!yearlyData[year]) {
        /* istanbul ignore next */
        cov_1fwcimt8ws().b[30][0]++;
        cov_1fwcimt8ws().s[142]++;
        yearlyData[year] = {
          totalReturn: 0,
          returnPercentage: 0,
          totalTrades: 0,
          avgWinRate: 0,
          bestMonth: -Infinity,
          worstMonth: Infinity,
          months: 0
        };
      } else
      /* istanbul ignore next */
      {
        cov_1fwcimt8ws().b[30][1]++;
      }
      const data =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[143]++, yearlyData[year]);
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[144]++;
      data.totalReturn += month.return;
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[145]++;
      data.returnPercentage += month.returnPercentage;
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[146]++;
      data.totalTrades += month.trades;
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[147]++;
      data.avgWinRate += month.winRate;
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[148]++;
      data.bestMonth = Math.max(data.bestMonth, month.returnPercentage);
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[149]++;
      data.worstMonth = Math.min(data.worstMonth, month.returnPercentage);
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[150]++;
      data.months++;
    }
    // Calculate averages
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[151]++;
    for (const data of Object.values(yearlyData)) {
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[152]++;
      data.avgWinRate /= data.months;
    }
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[153]++;
    return yearlyData;
  }
  static generateBenchmarkAnalysis(benchmark) {
    /* istanbul ignore next */
    cov_1fwcimt8ws().f[27]++;
    let analysis =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[154]++, '');
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[155]++;
    if (benchmark.outperformance.totalReturn > 0) {
      /* istanbul ignore next */
      cov_1fwcimt8ws().b[31][0]++;
      cov_1fwcimt8ws().s[156]++;
      analysis += `✅ The strategy **outperformed** the benchmark by ${benchmark.outperformance.totalReturn.toFixed(2)}%.\n`;
    } else {
      /* istanbul ignore next */
      cov_1fwcimt8ws().b[31][1]++;
      cov_1fwcimt8ws().s[157]++;
      analysis += `❌ The strategy **underperformed** the benchmark by ${Math.abs(benchmark.outperformance.totalReturn).toFixed(2)}%.\n`;
    }
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[158]++;
    if (benchmark.outperformance.riskAdjustedReturn > 0) {
      /* istanbul ignore next */
      cov_1fwcimt8ws().b[32][0]++;
      cov_1fwcimt8ws().s[159]++;
      analysis += `✅ **Risk-adjusted outperformance** of ${benchmark.outperformance.riskAdjustedReturn.toFixed(2)}% indicates superior risk management.\n`;
    } else {
      /* istanbul ignore next */
      cov_1fwcimt8ws().b[32][1]++;
      cov_1fwcimt8ws().s[160]++;
      analysis += `❌ **Risk-adjusted underperformance** of ${Math.abs(benchmark.outperformance.riskAdjustedReturn).toFixed(2)}% suggests poor risk management.\n`;
    }
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[161]++;
    if (benchmark.outperformance.informationRatio > 0.5) {
      /* istanbul ignore next */
      cov_1fwcimt8ws().b[33][0]++;
      cov_1fwcimt8ws().s[162]++;
      analysis += `✅ **High Information Ratio** (${benchmark.outperformance.informationRatio.toFixed(2)}) indicates consistent outperformance.\n`;
    } else {
      /* istanbul ignore next */
      cov_1fwcimt8ws().b[33][1]++;
      cov_1fwcimt8ws().s[163]++;
      if (benchmark.outperformance.informationRatio > 0) {
        /* istanbul ignore next */
        cov_1fwcimt8ws().b[34][0]++;
        cov_1fwcimt8ws().s[164]++;
        analysis += `⚠️ **Moderate Information Ratio** (${benchmark.outperformance.informationRatio.toFixed(2)}) shows some outperformance.\n`;
      } else {
        /* istanbul ignore next */
        cov_1fwcimt8ws().b[34][1]++;
        cov_1fwcimt8ws().s[165]++;
        analysis += `❌ **Negative Information Ratio** (${benchmark.outperformance.informationRatio.toFixed(2)}) indicates poor performance vs benchmark.\n`;
      }
    }
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[166]++;
    return analysis;
  }
  static markdownToHTML(markdown) {
    /* istanbul ignore next */
    cov_1fwcimt8ws().f[28]++;
    cov_1fwcimt8ws().s[167]++;
    return markdown.replace(/### (.*)/g, '<h3>$1</h3>').replace(/## (.*)/g, '<h2>$1</h2>').replace(/# (.*)/g, '<h1>$1</h1>').replace(/\*\*(.*?)\*\*/g, '<strong>$1</strong>').replace(/\*(.*?)\*/g, '<em>$1</em>').replace(/\n\n/g, '</p><p>').replace(/\n/g, '<br>').replace(/^/, '<p>').replace(/$/, '</p>').replace(/\|(.+)\|/g, (match, content) => {
      /* istanbul ignore next */
      cov_1fwcimt8ws().f[29]++;
      const cells =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[168]++, content.split('|').map(cell => {
        /* istanbul ignore next */
        cov_1fwcimt8ws().f[30]++;
        cov_1fwcimt8ws().s[169]++;
        return cell.trim();
      }));
      const isHeader =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[170]++, match.includes('---'));
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[171]++;
      if (isHeader) {
        /* istanbul ignore next */
        cov_1fwcimt8ws().b[35][0]++;
        cov_1fwcimt8ws().s[172]++;
        return '';
      } else
      /* istanbul ignore next */
      {
        cov_1fwcimt8ws().b[35][1]++;
      }
      const tag =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[173]++, cells[0] === cells[0].toUpperCase() ?
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[36][0]++, 'th') :
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[36][1]++, 'td'));
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[174]++;
      return `<tr>${cells.map(cell => {
        /* istanbul ignore next */
        cov_1fwcimt8ws().f[31]++;
        cov_1fwcimt8ws().s[175]++;
        return `<${tag}>${cell}</${tag}>`;
      }).join('')}</tr>`;
    }).replace(/(<tr>.*<\/tr>)/g, '<table>$1</table>').replace(/<\/table>\s*<table>/g, '');
  }
}
/* istanbul ignore next */
cov_1fwcimt8ws().s[176]++;
exports.BacktestReportGenerator = BacktestReportGenerator;