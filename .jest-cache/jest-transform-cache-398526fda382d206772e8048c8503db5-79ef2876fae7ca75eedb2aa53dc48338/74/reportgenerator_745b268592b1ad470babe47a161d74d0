      cov_1fwcimt8ws().b[20][0]++;
      cov_1fwcimt8ws().s[107]++;
      assessment += '✅ **Low** maximum drawdown (< 10%)\n';
    } else {
      /* istanbul ignore next */
      cov_1fwcimt8ws().b[20][1]++;
      cov_1fwcimt8ws().s[108]++;
