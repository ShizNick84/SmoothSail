  static generateRiskAssessment(result) {
    /* istanbul ignore next */
    cov_1fwcimt8ws().f[19]++;
    const risk =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[96]++, result.risk);
    const perf =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[97]++, result.performance);
    let assessment =
    /* istanbul ignore next */
    (cov_1fwcimt8ws().s[98]++, '');
    /* istanbul ignore next */
    cov_1fwcimt8ws().s[99]++;
    if (perf.sharpeRatio > 2) {
      /* istanbul ignore next */
      cov_1fwcimt8ws().b[17][0]++;
      cov_1fwcimt8ws().s[100]++;
      assessment += '✅ **Excellent** risk-adjusted returns (Sharpe > 2)\n';
    } else {
      /* istanbul ignore next */
      cov_1fwcimt8ws().b[17][1]++;
      cov_1fwcimt8ws().s[101]++;
