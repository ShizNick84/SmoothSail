      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[88]++, path_1.default.dirname(outputPath));
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[89]++;
      await promises_1.default.mkdir(dir, {
        recursive: true
      });
      // Determine file extension
      const ext =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[90]++, format.toLowerCase() === 'html' ?
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[14][0]++, '.html') :
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[14][1]++, format.toLowerCase() === 'json' ?
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[15][0]++, '.json') :
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[15][1]++, '.txt')));
      const fullPath =
      /* istanbul ignore next */
      (cov_1fwcimt8ws().s[91]++, outputPath.endsWith(ext) ?
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[16][0]++, outputPath) :
      /* istanbul ignore next */
      (cov_1fwcimt8ws().b[16][1]++, `${outputPath}${ext}`));
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[92]++;
      await promises_1.default.writeFile(fullPath, report, 'utf-8');
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[93]++;
      logger_1.logger.info(`📄 Report saved to: ${fullPath}`);
    } catch (error) {
      /* istanbul ignore next */
      cov_1fwcimt8ws().s[94]++;
      logger_1.logger.error('❌ Failed to save report:', error);
      /* istanbul ignore next */
