  }
  /**
   * Calculate volatility (standard deviation)
   */
  static calculateVolatility(returns) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[24]++;
    cov_t8e2po7h3().s[152]++;
    if (returns.length <= 1) {
      /* istanbul ignore next */
      cov_t8e2po7h3().b[31][0]++;
      cov_t8e2po7h3().s[153]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_t8e2po7h3().b[31][1]++;
    }
    const avgReturn =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[154]++, returns.reduce((sum, r) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[25]++;
      cov_t8e2po7h3().s[155]++;
      return sum + r;
    }, 0) / returns.length);
    const variance =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[156]++, returns.reduce((sum, r) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[26]++;
      cov_t8e2po7h3().s[157]++;
      return sum + Math.pow(r - avgReturn, 2);
    }, 0) / (returns.length - 1));
    /* istanbul ignore next */
    cov_t8e2po7h3().s[158]++;
    return Math.sqrt(variance);
  }
  /**
   * Calculate downside deviation
   */
  static calculateDownsideDeviation(returns, targetReturn) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[27]++;
    const downsideReturns =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[159]++, returns.filter(r => {
