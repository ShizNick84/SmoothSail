    (cov_t8e2po7h3().s[28]++, this.calculateVolatility(returns) * Math.sqrt(TRADING_DAYS_PER_YEAR));
    // Calculate downside deviation
    const downsideDeviation =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[29]++, this.calculateDownsideDeviation(returns, RISK_FREE_RATE / TRADING_DAYS_PER_YEAR));
    // Calculate VaR and CVaR
    const var95 =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[30]++, this.calculateVaR(returns, 0.95));
    const cvar95 =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[31]++, this.calculateCVaR(returns, 0.95));
    // Calculate Beta and Alpha (if benchmark provided)
    let beta =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[32]++, 0);
    let alpha =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[33]++, 0);
    let informationRatio =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[34]++, 0);
    /* istanbul ignore next */
    cov_t8e2po7h3().s[35]++;
    if (
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[4][0]++, benchmarkReturns) &&
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[4][1]++, benchmarkReturns.length === returns.length)) {
      /* istanbul ignore next */
      cov_t8e2po7h3().b[3][0]++;
      cov_t8e2po7h3().s[36]++;
      beta = this.calculateBeta(returns, benchmarkReturns);
      /* istanbul ignore next */
      cov_t8e2po7h3().s[37]++;
      alpha = this.calculateAlpha(returns, benchmarkReturns, RISK_FREE_RATE / TRADING_DAYS_PER_YEAR, beta);
      /* istanbul ignore next */
      cov_t8e2po7h3().s[38]++;
      informationRatio = this.calculateInformationRatio(returns, benchmarkReturns);
    } else
    /* istanbul ignore next */
    {
      cov_t8e2po7h3().b[3][1]++;
    }
    const performance =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[39]++, {
      totalReturn,
      totalReturnPercentage,
      annualizedReturn: annualizedReturn * 100,
      sharpeRatio,
      sortinoRatio,
      calmarRatio,
      maxDrawdown: Math.max(...portfolioHistory.map(p => {
        /* istanbul ignore next */
        cov_t8e2po7h3().f[3]++;
        cov_t8e2po7h3().s[40]++;
        return p.maxDrawdown;
      })),
      maxDrawdownPercentage: maxDrawdownPercent,
      averageDrawdown: this.calculateAverageDrawdown(portfolioHistory),
      recoveryFactor,
      profitFactor,
      payoffRatio,
      expectedValue
    });
    /* istanbul ignore next */
    cov_t8e2po7h3().s[41]++;
    logger_1.logger.info(' Performance metrics calculated successfully');
    /* istanbul ignore next */
    cov_t8e2po7h3().s[42]++;
    logger_1.logger.info(` Total Return: ${totalReturnPercentage.toFixed(2)}%`);
    /* istanbul ignore next */
    cov_t8e2po7h3().s[43]++;
    logger_1.logger.info(` Sharpe Ratio: ${sharpeRatio.toFixed(2)}`);
    /* istanbul ignore next */
    cov_t8e2po7h3().s[44]++;
    logger_1.logger.info(` Max Drawdown: ${maxDrawdownPercent.toFixed(2)}%`);
    /* istanbul ignore next */
    cov_t8e2po7h3().s[45]++;
    return performance;
  }
  /**
   * Calculate risk metrics
   */
  static calculateRiskMetrics(returns, benchmarkReturns) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[4]++;
    cov_t8e2po7h3().s[46]++;
    logger_1.logger.info(' Calculating risk metrics...');
    const volatility =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[47]++, this.calculateVolatility(returns) * Math.sqrt(TRADING_DAYS_PER_YEAR));
    const downsideDeviation =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[48]++, this.calculateDownsideDeviation(returns, RISK_FREE_RATE / TRADING_DAYS_PER_YEAR));
    const var95 =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[49]++, this.calculateVaR(returns, 0.95));
    const cvar95 =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[50]++, this.calculateCVaR(returns, 0.95));
    let beta =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[51]++, 0);
    let alpha =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[52]++, 0);
    let informationRatio =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[53]++, 0);
    /* istanbul ignore next */
    cov_t8e2po7h3().s[54]++;
    if (
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[6][0]++, benchmarkReturns) &&
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[6][1]++, benchmarkReturns.length === returns.length)) {
      /* istanbul ignore next */
      cov_t8e2po7h3().b[5][0]++;
      cov_t8e2po7h3().s[55]++;
      beta = this.calculateBeta(returns, benchmarkReturns);
      /* istanbul ignore next */
      cov_t8e2po7h3().s[56]++;
      alpha = this.calculateAlpha(returns, benchmarkReturns, RISK_FREE_RATE / TRADING_DAYS_PER_YEAR, beta);
      /* istanbul ignore next */
      cov_t8e2po7h3().s[57]++;
      informationRatio = this.calculateInformationRatio(returns, benchmarkReturns);
    } else
    /* istanbul ignore next */
    {
      cov_t8e2po7h3().b[5][1]++;
    }
    cov_t8e2po7h3().s[58]++;
    return {
      volatility: volatility * 100,
      downside_deviation: downsideDeviation * Math.sqrt(TRADING_DAYS_PER_YEAR) * 100,
      var95: var95 * 100,
      cvar95: cvar95 * 100,
      beta,
      alpha: alpha * 100,
      informationRatio
    };
  }
  /**
   * Calculate trade statistics
   */
  static calculateTradeStatistics(trades) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[5]++;
    cov_t8e2po7h3().s[59]++;
    logger_1.logger.info(' Calculating trade statistics...');
    const closedTrades =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[60]++, trades.filter(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[6]++;
      cov_t8e2po7h3().s[61]++;
      return /* istanbul ignore next */(cov_t8e2po7h3().b[7][0]++, t.status === 'CLOSED') &&
      /* istanbul ignore next */
      (cov_t8e2po7h3().b[7][1]++, t.pnl !== undefined);
    }));
    const winningTrades =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[62]++, closedTrades.filter(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[7]++;
      cov_t8e2po7h3().s[63]++;
      return t.pnl > 0;
    }));
    const losingTrades =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[64]++, closedTrades.filter(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[8]++;
      cov_t8e2po7h3().s[65]++;
      return t.pnl < 0;
    }));
    const winRate =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[66]++, closedTrades.length > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[8][0]++, winningTrades.length / closedTrades.length * 100) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[8][1]++, 0));
    const averageWin =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[67]++, winningTrades.length > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[9][0]++, winningTrades.reduce((sum, t) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[9]++;
      cov_t8e2po7h3().s[68]++;
      return sum + t.pnl;
    }, 0) / winningTrades.length) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[9][1]++, 0));
    const averageLoss =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[69]++, losingTrades.length > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[10][0]++, Math.abs(losingTrades.reduce((sum, t) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[10]++;
      cov_t8e2po7h3().s[70]++;
      return sum + t.pnl;
    }, 0) / losingTrades.length)) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[10][1]++, 0));
    const largestWin =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[71]++, winningTrades.length > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[11][0]++, Math.max(...winningTrades.map(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[11]++;
      cov_t8e2po7h3().s[72]++;
      return t.pnl;
    }))) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[11][1]++, 0));
    const largestLoss =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[73]++, losingTrades.length > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[12][0]++, Math.abs(Math.min(...losingTrades.map(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[12]++;
      cov_t8e2po7h3().s[74]++;
      return t.pnl;
    })))) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[12][1]++, 0));
    // Calculate consecutive wins/losses
    const {
      maxConsecutiveWins,
      maxConsecutiveLosses
    } =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[75]++, this.calculateConsecutiveWinsLosses(closedTrades));
    /* istanbul ignore next */
    cov_t8e2po7h3().s[76]++;
    return {
      total: closedTrades.length,
      winning: winningTrades.length,
      losing: losingTrades.length,
      winRate,
      averageWin,
      averageLoss,
      largestWin,
      largestLoss,
      consecutiveWins: 0,
      // Current streak (would be calculated in real-time)
      consecutiveLosses: 0,
      // Current streak (would be calculated in real-time)
      maxConsecutiveWins,
      maxConsecutiveLosses
    };
  }
  /**
   * Generate equity curve data
   */
  static generateEquityCurve(portfolioHistory) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[13]++;
    cov_t8e2po7h3().s[77]++;
    return portfolioHistory.map(portfolio => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[14]++;
      cov_t8e2po7h3().s[78]++;
      return {
        timestamp: portfolio.timestamp,
        equity: portfolio.equity,
        balance: portfolio.balance,
        unrealizedPnL: portfolio.unrealizedPnL,
        drawdown: portfolio.drawdown,
        drawdownPercentage: portfolio.drawdownPercentage
      };
    });
  }
  /**
   * Generate drawdown curve data
   */
  static generateDrawdownCurve(portfolioHistory) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[15]++;
    const drawdownCurve =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[79]++, []);
    let inDrawdown =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[80]++, false);
    let drawdownStart =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[81]++, null);
    /* istanbul ignore next */
    cov_t8e2po7h3().s[82]++;
    for (let i =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[83]++, 0); i < portfolioHistory.length; i++) {
      const portfolio =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[84]++, portfolioHistory[i]);
      const isUnderwater =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[85]++, portfolio.drawdownPercentage > 0);
      /* istanbul ignore next */
      cov_t8e2po7h3().s[86]++;
      if (
      /* istanbul ignore next */
      (cov_t8e2po7h3().b[14][0]++, !inDrawdown) &&
      /* istanbul ignore next */
      (cov_t8e2po7h3().b[14][1]++, isUnderwater)) {
        /* istanbul ignore next */
        cov_t8e2po7h3().b[13][0]++;
        cov_t8e2po7h3().s[87]++;
        inDrawdown = true;
        /* istanbul ignore next */
        cov_t8e2po7h3().s[88]++;
        drawdownStart = portfolio.timestamp;
      } else {
        /* istanbul ignore next */
        cov_t8e2po7h3().b[13][1]++;
        cov_t8e2po7h3().s[89]++;
        if (
        /* istanbul ignore next */
        (cov_t8e2po7h3().b[16][0]++, inDrawdown) &&
        /* istanbul ignore next */
        (cov_t8e2po7h3().b[16][1]++, !isUnderwater)) {
          /* istanbul ignore next */
          cov_t8e2po7h3().b[15][0]++;
          cov_t8e2po7h3().s[90]++;
          inDrawdown = false;
          // Calculate recovery time for previous drawdown points
          /* istanbul ignore next */
          cov_t8e2po7h3().s[91]++;
          if (drawdownStart) {
            /* istanbul ignore next */
            cov_t8e2po7h3().b[17][0]++;
            const recoveryTime =
            /* istanbul ignore next */
            (cov_t8e2po7h3().s[92]++, portfolio.timestamp.getTime() - drawdownStart.getTime());
            /* istanbul ignore next */
            cov_t8e2po7h3().s[93]++;
            for (let j =
            /* istanbul ignore next */
            (cov_t8e2po7h3().s[94]++, drawdownCurve.length - 1); j >= 0; j--) {
              /* istanbul ignore next */
              cov_t8e2po7h3().s[95]++;
              if (drawdownCurve[j].timestamp >= drawdownStart) {
                /* istanbul ignore next */
                cov_t8e2po7h3().b[18][0]++;
                cov_t8e2po7h3().s[96]++;
                drawdownCurve[j].recoveryTime = recoveryTime;
              } else {
                /* istanbul ignore next */
                cov_t8e2po7h3().b[18][1]++;
                cov_t8e2po7h3().s[97]++;
                break;
              }
            }
          } else
          /* istanbul ignore next */
          {
            cov_t8e2po7h3().b[17][1]++;
          }
          cov_t8e2po7h3().s[98]++;
          drawdownStart = null;
        } else
        /* istanbul ignore next */
        {
          cov_t8e2po7h3().b[15][1]++;
        }
      }
      /* istanbul ignore next */
      cov_t8e2po7h3().s[99]++;
      drawdownCurve.push({
        timestamp: portfolio.timestamp,
        drawdown: portfolio.drawdown,
        drawdownPercentage: portfolio.drawdownPercentage,
        underwater: isUnderwater
      });
    }
    /* istanbul ignore next */
    cov_t8e2po7h3().s[100]++;
    return drawdownCurve;
  }
  /**
   * Generate monthly returns breakdown
   */
  static generateMonthlyReturns(portfolioHistory, trades) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[16]++;
    const monthlyReturns =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[101]++, []);
    const monthlyData =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[102]++, new Map());
    // Group data by month
    /* istanbul ignore next */
    cov_t8e2po7h3().s[103]++;
    for (const portfolio of portfolioHistory) {
      const monthKey =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[104]++, `${portfolio.timestamp.getFullYear()}-${portfolio.timestamp.getMonth()}`);
      /* istanbul ignore next */
      cov_t8e2po7h3().s[105]++;
      if (!monthlyData.has(monthKey)) {
        /* istanbul ignore next */
        cov_t8e2po7h3().b[19][0]++;
        cov_t8e2po7h3().s[106]++;
        monthlyData.set(monthKey, {
          startEquity: portfolio.equity,
          endEquity: portfolio.equity,
          trades: [],
          maxDrawdown: 0
        });
      } else {
        /* istanbul ignore next */
        cov_t8e2po7h3().b[19][1]++;
        const data =
        /* istanbul ignore next */
        (cov_t8e2po7h3().s[107]++, monthlyData.get(monthKey));
        /* istanbul ignore next */
        cov_t8e2po7h3().s[108]++;
        data.endEquity = portfolio.equity;
        /* istanbul ignore next */
        cov_t8e2po7h3().s[109]++;
        data.maxDrawdown = Math.max(data.maxDrawdown, portfolio.drawdownPercentage);
      }
    }
    // Group trades by month
    /* istanbul ignore next */
    cov_t8e2po7h3().s[110]++;
    for (const trade of trades) {
      /* istanbul ignore next */
      cov_t8e2po7h3().s[111]++;
      if (trade.exitTime) {
        /* istanbul ignore next */
        cov_t8e2po7h3().b[20][0]++;
        const monthKey =
        /* istanbul ignore next */
        (cov_t8e2po7h3().s[112]++, `${trade.exitTime.getFullYear()}-${trade.exitTime.getMonth()}`);
        const data =
        /* istanbul ignore next */
        (cov_t8e2po7h3().s[113]++, monthlyData.get(monthKey));
        /* istanbul ignore next */
        cov_t8e2po7h3().s[114]++;
        if (data) {
          /* istanbul ignore next */
          cov_t8e2po7h3().b[21][0]++;
          cov_t8e2po7h3().s[115]++;
          data.trades.push(trade);
        } else
        /* istanbul ignore next */
        {
          cov_t8e2po7h3().b[21][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_t8e2po7h3().b[20][1]++;
      }
    }
    // Calculate monthly returns
    /* istanbul ignore next */
    cov_t8e2po7h3().s[116]++;
    for (const [monthKey, data] of monthlyData) {
      const [year, month] =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[117]++, monthKey.split('-').map(Number));
      const returnAmount =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[118]++, data.endEquity - data.startEquity);
      const returnPercentage =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[119]++, data.startEquity > 0 ?
      /* istanbul ignore next */
      (cov_t8e2po7h3().b[22][0]++, returnAmount / data.startEquity * 100) :
      /* istanbul ignore next */
      (cov_t8e2po7h3().b[22][1]++, 0));
      const winningTrades =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[120]++, data.trades.filter(t => {
        /* istanbul ignore next */
        cov_t8e2po7h3().f[17]++;
        cov_t8e2po7h3().s[121]++;
        return /* istanbul ignore next */(cov_t8e2po7h3().b[23][0]++, t.pnl) &&
        /* istanbul ignore next */
        (cov_t8e2po7h3().b[23][1]++, t.pnl > 0);
      }));
      const winRate =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[122]++, data.trades.length > 0 ?
      /* istanbul ignore next */
      (cov_t8e2po7h3().b[24][0]++, winningTrades.length / data.trades.length * 100) :
      /* istanbul ignore next */
      (cov_t8e2po7h3().b[24][1]++, 0));
      /* istanbul ignore next */
      cov_t8e2po7h3().s[123]++;
      monthlyReturns.push({
        year,
        month,
        return: returnAmount,
        returnPercentage,
        trades: data.trades.length,
        winRate,
        maxDrawdown: data.maxDrawdown
      });
    }
    /* istanbul ignore next */
    cov_t8e2po7h3().s[124]++;
    return monthlyReturns.sort((a, b) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[18]++;
      cov_t8e2po7h3().s[125]++;
      if (a.year !== b.year) {
        /* istanbul ignore next */
        cov_t8e2po7h3().b[25][0]++;
        cov_t8e2po7h3().s[126]++;
        return a.year - b.year;
      } else
      /* istanbul ignore next */
      {
        cov_t8e2po7h3().b[25][1]++;
      }
      cov_t8e2po7h3().s[127]++;
      return a.month - b.month;
    });
  }
  /**
   * Calculate returns array from portfolio history
   */
  static calculateReturns(portfolioHistory) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[19]++;
    const returns =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[128]++, []);
    /* istanbul ignore next */
    cov_t8e2po7h3().s[129]++;
    for (let i =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[130]++, 1); i < portfolioHistory.length; i++) {
      const prevEquity =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[131]++, portfolioHistory[i - 1].equity);
      const currEquity =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[132]++, portfolioHistory[i].equity);
      /* istanbul ignore next */
      cov_t8e2po7h3().s[133]++;
      if (prevEquity > 0) {
        /* istanbul ignore next */
        cov_t8e2po7h3().b[26][0]++;
        const returnRate =
        /* istanbul ignore next */
        (cov_t8e2po7h3().s[134]++, (currEquity - prevEquity) / prevEquity);
        /* istanbul ignore next */
        cov_t8e2po7h3().s[135]++;
        returns.push(returnRate);
      } else
      /* istanbul ignore next */
      {
        cov_t8e2po7h3().b[26][1]++;
      }
    }
    /* istanbul ignore next */
    cov_t8e2po7h3().s[136]++;
    return returns;
  }
  /**
   * Calculate Sharpe ratio
   */
  static calculateSharpeRatio(returns, riskFreeRate) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[20]++;
    cov_t8e2po7h3().s[137]++;
    if (returns.length === 0) {
      /* istanbul ignore next */
      cov_t8e2po7h3().b[27][0]++;
      cov_t8e2po7h3().s[138]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_t8e2po7h3().b[27][1]++;
    }
    const avgReturn =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[139]++, returns.reduce((sum, r) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[21]++;
      cov_t8e2po7h3().s[140]++;
      return sum + r;
    }, 0) / returns.length);
    const annualizedReturn =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[141]++, avgReturn * TRADING_DAYS_PER_YEAR);
    const volatility =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[142]++, this.calculateVolatility(returns) * Math.sqrt(TRADING_DAYS_PER_YEAR));
    /* istanbul ignore next */
    cov_t8e2po7h3().s[143]++;
    return volatility > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[28][0]++, (annualizedReturn - riskFreeRate) / volatility) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[28][1]++, 0);
  }
  /**
