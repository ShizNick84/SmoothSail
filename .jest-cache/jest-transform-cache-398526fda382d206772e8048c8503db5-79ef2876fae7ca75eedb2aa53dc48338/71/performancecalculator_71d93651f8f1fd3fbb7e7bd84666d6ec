  /**
   * Calculate average drawdown
   */
  static calculateAverageDrawdown(portfolioHistory) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[59]++;
    const drawdowns =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[240]++, portfolioHistory.map(p => {
      /* istanbul ignore next */
