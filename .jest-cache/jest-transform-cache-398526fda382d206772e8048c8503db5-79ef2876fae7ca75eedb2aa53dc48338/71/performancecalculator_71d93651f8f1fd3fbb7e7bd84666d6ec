      /* istanbul ignore next */
      (cov_t8e2po7h3().b[44][1]++, t.pnl !== undefined);
    }));
    const grossProfit =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[216]++, closedTrades.filter(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[46]++;
      cov_t8e2po7h3().s[217]++;
