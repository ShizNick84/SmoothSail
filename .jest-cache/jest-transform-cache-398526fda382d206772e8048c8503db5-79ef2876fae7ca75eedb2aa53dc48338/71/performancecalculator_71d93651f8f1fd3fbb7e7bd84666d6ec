  /**
   * Calculate Value at Risk (VaR)
   */
  static calculateVaR(returns, confidence) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[30]++;
    cov_t8e2po7h3().s[166]++;
    if (returns.length === 0) {
      /* istanbul ignore next */
      cov_t8e2po7h3().b[33][0]++;
      cov_t8e2po7h3().s[167]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_t8e2po7h3().b[33][1]++;
    }
    const sortedReturns =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[168]++, [...returns].sort((a, b) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[31]++;
      cov_t8e2po7h3().s[169]++;
      return a - b;
    }));
    const index =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[170]++, Math.floor((1 - confidence) * sortedReturns.length));
    /* istanbul ignore next */
    cov_t8e2po7h3().s[171]++;
    return /* istanbul ignore next */(cov_t8e2po7h3().b[34][0]++, sortedReturns[index]) ||
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[34][1]++, 0);
  }
  /**
   * Calculate Conditional Value at Risk (CVaR)
   */
  static calculateCVaR(returns, confidence) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[32]++;
    cov_t8e2po7h3().s[172]++;
    if (returns.length === 0) {
      /* istanbul ignore next */
      cov_t8e2po7h3().b[35][0]++;
      cov_t8e2po7h3().s[173]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_t8e2po7h3().b[35][1]++;
    }
    const sortedReturns =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[174]++, [...returns].sort((a, b) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[33]++;
      cov_t8e2po7h3().s[175]++;
      return a - b;
    }));
    const cutoffIndex =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[176]++, Math.floor((1 - confidence) * sortedReturns.length));
    const tailReturns =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[177]++, sortedReturns.slice(0, cutoffIndex + 1));
    /* istanbul ignore next */
    cov_t8e2po7h3().s[178]++;
    if (tailReturns.length === 0) {
      /* istanbul ignore next */
      cov_t8e2po7h3().b[36][0]++;
      cov_t8e2po7h3().s[179]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_t8e2po7h3().b[36][1]++;
    }
    cov_t8e2po7h3().s[180]++;
    return tailReturns.reduce((sum, r) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[34]++;
      cov_t8e2po7h3().s[181]++;
      return sum + r;
    }, 0) / tailReturns.length;
  }
  /**
   * Calculate Beta (correlation with benchmark)
   */
  static calculateBeta(returns, benchmarkReturns) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[35]++;
    cov_t8e2po7h3().s[182]++;
    if (
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[38][0]++, returns.length !== benchmarkReturns.length) ||
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[38][1]++, returns.length === 0)) {
      /* istanbul ignore next */
      cov_t8e2po7h3().b[37][0]++;
      cov_t8e2po7h3().s[183]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_t8e2po7h3().b[37][1]++;
    }
    const avgReturn =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[184]++, returns.reduce((sum, r) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[36]++;
      cov_t8e2po7h3().s[185]++;
      return sum + r;
    }, 0) / returns.length);
    const avgBenchmark =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[186]++, benchmarkReturns.reduce((sum, r) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[37]++;
      cov_t8e2po7h3().s[187]++;
      return sum + r;
    }, 0) / benchmarkReturns.length);
    let covariance =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[188]++, 0);
    let benchmarkVariance =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[189]++, 0);
    /* istanbul ignore next */
    cov_t8e2po7h3().s[190]++;
    for (let i =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[191]++, 0); i < returns.length; i++) {
      const returnDiff =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[192]++, returns[i] - avgReturn);
      const benchmarkDiff =
      /* istanbul ignore next */
      (cov_t8e2po7h3().s[193]++, benchmarkReturns[i] - avgBenchmark);
      /* istanbul ignore next */
      cov_t8e2po7h3().s[194]++;
      covariance += returnDiff * benchmarkDiff;
      /* istanbul ignore next */
      cov_t8e2po7h3().s[195]++;
      benchmarkVariance += benchmarkDiff * benchmarkDiff;
    }
    /* istanbul ignore next */
    cov_t8e2po7h3().s[196]++;
    covariance /= returns.length - 1;
    /* istanbul ignore next */
    cov_t8e2po7h3().s[197]++;
    benchmarkVariance /= returns.length - 1;
    /* istanbul ignore next */
    cov_t8e2po7h3().s[198]++;
    return benchmarkVariance > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[39][0]++, covariance / benchmarkVariance) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[39][1]++, 0);
  }
  /**
   * Calculate Alpha (excess return over benchmark)
   */
  static calculateAlpha(returns, benchmarkReturns, riskFreeRate, beta) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[38]++;
    cov_t8e2po7h3().s[199]++;
    if (returns.length === 0) {
      /* istanbul ignore next */
      cov_t8e2po7h3().b[40][0]++;
      cov_t8e2po7h3().s[200]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_t8e2po7h3().b[40][1]++;
    }
    const avgReturn =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[201]++, returns.reduce((sum, r) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[39]++;
      cov_t8e2po7h3().s[202]++;
      return sum + r;
    }, 0) / returns.length);
    const avgBenchmark =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[203]++, benchmarkReturns.reduce((sum, r) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[40]++;
      cov_t8e2po7h3().s[204]++;
      return sum + r;
    }, 0) / benchmarkReturns.length);
    /* istanbul ignore next */
    cov_t8e2po7h3().s[205]++;
    return avgReturn - (riskFreeRate + beta * (avgBenchmark - riskFreeRate));
  }
  /**
   * Calculate Information Ratio
   */
  static calculateInformationRatio(returns, benchmarkReturns) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[41]++;
    cov_t8e2po7h3().s[206]++;
