    }).reduce((sum, t) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[47]++;
      cov_t8e2po7h3().s[218]++;
      return sum + t.pnl;
    }, 0));
    const grossLoss =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[219]++, Math.abs(closedTrades.filter(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[48]++;
      cov_t8e2po7h3().s[220]++;
      return t.pnl < 0;
    }).reduce((sum, t) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[49]++;
      cov_t8e2po7h3().s[221]++;
      return sum + t.pnl;
    }, 0)));
    /* istanbul ignore next */
    cov_t8e2po7h3().s[222]++;
    return grossLoss > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[45][0]++, grossProfit / grossLoss) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[45][1]++, grossProfit > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[46][0]++, Infinity) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[46][1]++, 0));
  }
  /**
   * Calculate payoff ratio (average win / average loss)
   */
  static calculatePayoffRatio(trades) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[50]++;
    const closedTrades =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[223]++, trades.filter(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[51]++;
      cov_t8e2po7h3().s[224]++;
      return /* istanbul ignore next */(cov_t8e2po7h3().b[47][0]++, t.status === 'CLOSED') &&
      /* istanbul ignore next */
      (cov_t8e2po7h3().b[47][1]++, t.pnl !== undefined);
    }));
    const winningTrades =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[225]++, closedTrades.filter(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[52]++;
      cov_t8e2po7h3().s[226]++;
      return t.pnl > 0;
    }));
    const losingTrades =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[227]++, closedTrades.filter(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[53]++;
      cov_t8e2po7h3().s[228]++;
      return t.pnl < 0;
    }));
    const averageWin =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[229]++, winningTrades.length > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[48][0]++, winningTrades.reduce((sum, t) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[54]++;
      cov_t8e2po7h3().s[230]++;
      return sum + t.pnl;
    }, 0) / winningTrades.length) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[48][1]++, 0));
    const averageLoss =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[231]++, losingTrades.length > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[49][0]++, Math.abs(losingTrades.reduce((sum, t) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[55]++;
      cov_t8e2po7h3().s[232]++;
      return sum + t.pnl;
    }, 0) / losingTrades.length)) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[49][1]++, 0));
    /* istanbul ignore next */
    cov_t8e2po7h3().s[233]++;
    return averageLoss > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[50][0]++, averageWin / averageLoss) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[50][1]++, averageWin > 0 ?
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[51][0]++, Infinity) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[51][1]++, 0));
  }
