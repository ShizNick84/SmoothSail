    /* istanbul ignore next */
    {
      cov_t8e2po7h3().b[41][1]++;
    }
    const excessReturns =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[208]++, returns.map((r, i) => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[42]++;
      cov_t8e2po7h3().s[209]++;
