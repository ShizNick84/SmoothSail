    /* istanbul ignore next */
    (cov_t8e2po7h3().b[43][0]++, avgExcessReturn / trackingError) :
    /* istanbul ignore next */
    (cov_t8e2po7h3().b[43][1]++, 0);
  }
  /**
   * Calculate profit factor
   */
  static calculateProfitFactor(trades) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[44]++;
    const closedTrades =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[214]++, trades.filter(t => {
      /* istanbul ignore next */
      cov_t8e2po7h3().f[45]++;
      cov_t8e2po7h3().s[215]++;
