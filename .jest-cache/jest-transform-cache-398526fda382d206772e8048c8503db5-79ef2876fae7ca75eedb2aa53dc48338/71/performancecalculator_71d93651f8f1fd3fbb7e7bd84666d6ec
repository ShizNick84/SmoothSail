    }, 0) / drawdowns.length;
  }
  /**
   * Calculate consecutive wins and losses
   */
  static calculateConsecutiveWinsLosses(trades) {
    /* istanbul ignore next */
    cov_t8e2po7h3().f[63]++;
    let maxConsecutiveWins =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[247]++, 0);
    let maxConsecutiveLosses =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[248]++, 0);
    let currentWins =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[249]++, 0);
    let currentLosses =
    /* istanbul ignore next */
    (cov_t8e2po7h3().s[250]++, 0);
    /* istanbul ignore next */
    cov_t8e2po7h3().s[251]++;
