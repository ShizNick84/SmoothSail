  cov_1ejy8xhn1q().f[20]++;
  cov_1ejy8xhn1q().s[157]++;
  try {
    /* istanbul ignore next */
    cov_1ejy8xhn1q().s[158]++;
    logger_1.logger.error(' EMERGENCY SHUTDOWN PROCEDURES INITIATED');
    // Force close all positions immediately
    /* istanbul ignore next */
    cov_1ejy8xhn1q().s[159]++;
