    /* istanbul ignore next */
    (cov_1ejy8xhn1q().s[81]++, 15000); // 15 seconds for position closure
    const closePromise =
    /* istanbul ignore next */
    (cov_1ejy8xhn1q().s[82]++, new Promise(resolve => {
      /* istanbul ignore next */
      cov_1ejy8xhn1q().f[6]++;
      cov_1ejy8xhn1q().s[83]++;
