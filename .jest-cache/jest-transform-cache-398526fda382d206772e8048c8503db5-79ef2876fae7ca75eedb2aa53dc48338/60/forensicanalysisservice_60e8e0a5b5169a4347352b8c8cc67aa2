      /* istanbul ignore next */
      {
        cov_1s8bbcmgr7().b[8][1]++;
      }
      cov_1s8bbcmgr7().s[92]++;
      logger_1.logger.info(' Evidence integrity verification', {
        evidenceId,
        integrityVerified,
        status: evidence.integrityStatus
      });
      /* istanbul ignore next */
      cov_1s8bbcmgr7().s[93]++;
      return integrityVerified;
    } catch (error) {
      /* istanbul ignore next */
