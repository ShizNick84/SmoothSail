    /* istanbul ignore next */
    cov_1s8bbcmgr7().f[16]++;
    cov_1s8bbcmgr7().s[102]++;
    return (0, crypto_1.createHash)('sha256').update(Math.random().toString() + Date.now().toString()).digest('hex').substring(0, length * 2);
  }
