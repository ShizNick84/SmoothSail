      /* istanbul ignore next */
      (cov_2i2u7rvvp().s[101]++, await this.reducePositionSizes(positions, drawdownStatus.riskReductionLevel));
      /* istanbul ignore next */
      cov_2i2u7rvvp().s[102]++;
      actions.push(action);
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[28][1]++;
    }
    cov_2i2u7rvvp().s[103]++;
    this.emergencyActions.push(...actions);
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[104]++;
    return actions;
  }
  /**
   * Activate emergency stop
   */
  async activateEmergencyStop(positions) {
    /* istanbul ignore next */
    cov_2i2u7rvvp().f[18]++;
    cov_2i2u7rvvp().s[105]++;
    this.emergencyModeActive = true;
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[106]++;
    this.tradingHalted = true;
    // Mark current drawdown period as having emergency measures
    const currentDrawdown =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[107]++, this.getCurrentDrawdownPeriod());
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[108]++;
    if (currentDrawdown) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[30][0]++;
