      /* istanbul ignore next */
      (cov_2i2u7rvvp().s[87]++, positions.reduce((sum, p) => {
        /* istanbul ignore next */
        cov_2i2u7rvvp().f[15]++;
        cov_2i2u7rvvp().s[88]++;
