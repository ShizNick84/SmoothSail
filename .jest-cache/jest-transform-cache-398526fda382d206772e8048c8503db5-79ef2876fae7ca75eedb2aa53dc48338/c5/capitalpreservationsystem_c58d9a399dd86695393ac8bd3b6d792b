    this.tradingHalted = false;
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[133]++;
    this.consecutiveLosses = 0;
    // Add recovery alert
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[134]++;
    this.alerts.push({
      id: `recovery-${Date.now()}`,
      type: 'DRAWDOWN_WARNING',
      severity: 'LOW',
      message: 'Recovery conditions met. Normal operations resumed.',
      recommendedActions: ['Monitor performance closely'],
      timestamp: new Date(),
      autoExecutedActions: ['Emergency mode deactivated', 'Trading resumed']
    });
  }
  /**
   * Get capital preservation statistics
   */
  getCapitalPreservationStats() {
    /* istanbul ignore next */
    cov_2i2u7rvvp().f[27]++;
    const completedDrawdowns =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[135]++, this.drawdownPeriods.filter(d => {
      /* istanbul ignore next */
      cov_2i2u7rvvp().f[28]++;
      cov_2i2u7rvvp().s[136]++;
      return d.endDate !== null;
    }));
    const averageDrawdownDuration =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[137]++, completedDrawdowns.length > 0 ?
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[38][0]++, completedDrawdowns.reduce((sum, d) => {
      /* istanbul ignore next */
      cov_2i2u7rvvp().f[29]++;
      cov_2i2u7rvvp().s[138]++;
      return sum + d.durationDays;
    }, 0) / completedDrawdowns.length) :
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[38][1]++, 0));
    const maxHistoricalDrawdown =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[139]++, Math.max(...this.drawdownPeriods.map(d => {
      /* istanbul ignore next */
      cov_2i2u7rvvp().f[30]++;
      cov_2i2u7rvvp().s[140]++;
      return d.maxDrawdownPercent;
    }), 0));
    const emergencyActivations =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[141]++, this.drawdownPeriods.filter(d => {
      /* istanbul ignore next */
      cov_2i2u7rvvp().f[31]++;
      cov_2i2u7rvvp().s[142]++;
      return d.emergencyMeasuresActivated;
    }).length);
    const recoveryRate =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[143]++, completedDrawdowns.length > 0 ?
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[39][0]++, completedDrawdowns.length / this.drawdownPeriods.length * 100) :
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[39][1]++, 100));
    let currentStatus =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[144]++, 'NORMAL');
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[145]++;
    if (this.emergencyModeActive) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[40][0]++;
      cov_2i2u7rvvp().s[146]++;
      currentStatus = 'EMERGENCY';
    } else {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[40][1]++;
      cov_2i2u7rvvp().s[147]++;
      if (this.tradingHalted) {
        /* istanbul ignore next */
        cov_2i2u7rvvp().b[41][0]++;
        cov_2i2u7rvvp().s[148]++;
        currentStatus = 'HALTED';
      } else
      /* istanbul ignore next */
      {
        cov_2i2u7rvvp().b[41][1]++;
      }
    }
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[149]++;
    return {
      totalDrawdownPeriods: this.drawdownPeriods.length,
      averageDrawdownDuration,
      maxHistoricalDrawdown,
      emergencyActivations,
      recoveryRate,
      currentStatus
    };
  }
