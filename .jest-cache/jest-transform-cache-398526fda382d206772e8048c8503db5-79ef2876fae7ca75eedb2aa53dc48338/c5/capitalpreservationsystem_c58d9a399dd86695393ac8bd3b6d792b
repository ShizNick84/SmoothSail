  }
  /**
   * Determine if trading should be allowed
   */
  shouldAllowTrading(drawdownStatus, lossLimits) {
    /* istanbul ignore next */
    cov_2i2u7rvvp().f[23]++;
    cov_2i2u7rvvp().s[117]++;
    // Trading halted by emergency measures
    if (
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[32][0]++, this.tradingHalted) ||
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[32][1]++, this.emergencyModeActive)) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[31][0]++;
      cov_2i2u7rvvp().s[118]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[31][1]++;
    }
    // Critical drawdown
    cov_2i2u7rvvp().s[119]++;
    if (drawdownStatus.currentDrawdown >= this.config.criticalDrawdownThreshold) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[33][0]++;
      cov_2i2u7rvvp().s[120]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[33][1]++;
    }
    // Daily loss limit exceeded
    cov_2i2u7rvvp().s[121]++;
    if (lossLimits.daily.remaining <= 0) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[34][0]++;
      cov_2i2u7rvvp().s[122]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[34][1]++;
    }
    cov_2i2u7rvvp().s[123]++;
    return true;
  }
  /**
   * Calculate recommended position size adjustment
   */
  calculatePositionSizeAdjustment(basePositionSize, drawdownStatus) {
    /* istanbul ignore next */
    cov_2i2u7rvvp().f[24]++;
    cov_2i2u7rvvp().s[124]++;
    if (drawdownStatus.riskReductionLevel === 0) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[35][0]++;
      cov_2i2u7rvvp().s[125]++;
      return basePositionSize;
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[35][1]++;
    }
    const reductionFactor =
    /* istanbul ignore next */
    (cov_2i2u7rvvp().s[126]++, 1 - drawdownStatus.riskReductionLevel / 100);
    /* istanbul ignore next */
    cov_2i2u7rvvp().s[127]++;
    return basePositionSize * reductionFactor;
  }
  /**
   * Check if recovery conditions are met to resume normal operations
   */
  checkRecoveryConditions(drawdownStatus) {
    /* istanbul ignore next */
    cov_2i2u7rvvp().f[25]++;
    cov_2i2u7rvvp().s[128]++;
    if (!this.emergencyModeActive) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[36][0]++;
      cov_2i2u7rvvp().s[129]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[36][1]++;
    }
    // Recovery conditions:
    // 1. Drawdown below recovery threshold
    // 2. Recovery progress above threshold
    cov_2i2u7rvvp().s[130]++;
    return /* istanbul ignore next */(cov_2i2u7rvvp().b[37][0]++, drawdownStatus.currentDrawdown < this.config.recoveryThreshold) &&
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[37][1]++, drawdownStatus.recoveryProgress > 80);
  }
  /**
   * Resume normal operations after recovery
   */
  resumeNormalOperations() {
    /* istanbul ignore next */
    cov_2i2u7rvvp().f[26]++;
    cov_2i2u7rvvp().s[131]++;
    this.emergencyModeActive = false;
    /* istanbul ignore next */
