    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[23][0]++, btcPositions.length > 0) &&
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[23][1]++, ethPositions.length > 0)) {
