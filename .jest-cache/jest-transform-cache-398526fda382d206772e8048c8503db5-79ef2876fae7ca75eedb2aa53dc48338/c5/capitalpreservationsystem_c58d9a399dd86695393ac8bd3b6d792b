    (cov_2i2u7rvvp().b[25][0]++, drawdownStatus.currentDrawdown >= this.config.criticalDrawdownThreshold) &&
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[25][1]++, !this.emergencyModeActive)) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[24][0]++;
      const action =
      /* istanbul ignore next */
      (cov_2i2u7rvvp().s[95]++, await this.activateEmergencyStop(positions));
      /* istanbul ignore next */
      cov_2i2u7rvvp().s[96]++;
      actions.push(action);
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[24][1]++;
    }
    // Daily loss limit exceeded - halt trading
    cov_2i2u7rvvp().s[97]++;
    if (
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[27][0]++, lossLimits.daily.remaining <= 0) &&
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[27][1]++, !this.tradingHalted)) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[26][0]++;
      const action =
      /* istanbul ignore next */
      (cov_2i2u7rvvp().s[98]++, await this.haltDailyTrading());
      /* istanbul ignore next */
      cov_2i2u7rvvp().s[99]++;
      actions.push(action);
    } else
    /* istanbul ignore next */
    {
      cov_2i2u7rvvp().b[26][1]++;
    }
    // High drawdown - reduce position sizes
    cov_2i2u7rvvp().s[100]++;
    if (
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[29][0]++, drawdownStatus.currentDrawdown >= this.config.maxDrawdownThreshold) &&
    /* istanbul ignore next */
    (cov_2i2u7rvvp().b[29][1]++, drawdownStatus.riskReductionLevel > 0)) {
      /* istanbul ignore next */
      cov_2i2u7rvvp().b[28][0]++;
