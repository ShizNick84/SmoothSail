{
  "name": "ai-crypto-trading-agent",
  "version": "1.0.0",
  "description": "Military-grade AI cryptocurrency trading agent optimized for Intel NUC with capital preservation and profit maximization",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx watch src/main-simple.ts",
    "dev:full": "tsx watch src/main.ts",
    "dev:old": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "build:simple": "tsc src/main-simple.ts --outDir dist --target ES2022 --module commonjs --esModuleInterop --skipLibCheck",
    "start": "tsx src/main-simple.ts",
    "start:full": "node dist/main.js",
    "start:tunnel": "node dist/main.js",
    "start:linux": "./scripts/start-with-tunnel.sh",
    "tunnel:start:linux": "./start-tunnel.sh",
    "tunnel:stop:linux": "./stop-tunnel.sh",
    "test:ssh": "node scripts/test-ssh.js",
    "test:api": "node test-gateio-api.js",
    "dashboard:dev": "cd src/dashboard && next dev -p 3000",
    "dashboard:build": "cd src/dashboard && next build",
    "dashboard:start": "cd src/dashboard && next start -p 3000",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "npm audit && npm run test:security:custom",
    "test:security:custom": "tsx src/tests/security/security-audit.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "tunnel:start": "tsx src/scripts/tunnel-start.ts",
    "tunnel:stop": "tsx src/scripts/tunnel-stop.ts",
    "tunnel:status": "tsx src/scripts/tunnel-status.ts",
    "system:monitor": "tsx src/scripts/system-monitor.ts",
    "trading:start": "tsx src/scripts/trading-start.ts",
    "trading:stop": "tsx src/scripts/trading-stop.ts",
    "security:scan": "tsx src/scripts/security-scan.ts",
    "backup:create": "tsx src/scripts/backup-create.ts",
    "deploy:production": "tsx src/scripts/deployment-cli.ts deploy",
    "deploy:staging": "tsx src/scripts/deployment-cli.ts deploy --environment staging",
    "deploy:validate": "tsx src/scripts/deployment-cli.ts validate",
    "deploy:test": "tsx src/scripts/deployment-cli.ts test",
    "deploy:security": "tsx src/scripts/deployment-cli.ts security",
    "deploy:status": "tsx src/scripts/deployment-cli.ts status",
    "deploy:restart": "tsx src/scripts/deployment-cli.ts restart",
    "deploy:stop": "tsx src/scripts/deployment-cli.ts stop",
    "deploy:backup": "tsx src/scripts/deployment-cli.ts backup",
    "deploy:health": "tsx src/scripts/deployment-cli.ts health",
    "deploy:logs": "tsx src/scripts/deployment-cli.ts logs",
    "validate:env": "tsx src/scripts/validate-environment.ts",
    "validate:full": "tsx src/scripts/deployment-cli.ts validate",
    "test:e2e": "tsx src/scripts/deployment-cli.ts test",
    "config:validate": "tsx src/scripts/deployment-cli.ts config --validate",
    "config:backup": "tsx src/scripts/deployment-cli.ts config --backup"
  },
  "keywords": [
    "cryptocurrency",
    "trading",
    "ai",
    "security",
    "intel-nuc",
    "gate.io",
    "oracle-cloud"
  ],
  "author": "AI Crypto Trading System",
  "license": "PROPRIETARY",
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
